CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ProductDelivery-management.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div class=\"warehouse-management-container\"\r\n       id=\"warehouse\">\r\n    <!-- <h1>出库管理</h1> -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <div class=\"materials-apply-add\">\r\n        <div class=\"add-left\">\r\n          <p class=\"title-font\">成品清单</p>\r\n          <el-table :data=\"deliveryData.stockInIdList\"\r\n                    style=\"width: 100%\"\r\n                    border\r\n                    id=\"left\">\r\n                  <el-table-column prop=\"designName\" label=\"设计图名称\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.designName==\"\"?'---':scope.row.designName}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n            <el-table-column prop=\"productNumber\"\r\n                             label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column label=\"移除材料\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"num\">\r\n                  <el-button type=\"text\"\r\n                             style=\"color: red;width: 100%;\"\r\n                             @click=\"delNum(scope.$index)\">移除</el-button>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n\r\n          <el-form style=\"margin-top: 20px;\"\r\n                   :model=\"deliveryData\"\r\n                   :rules=\"deliveryRules\" \r\n                   ref=\"deliveryForm\"\r\n                   label-width=\"110px\">\r\n\r\n            <el-form-item label=\"出库类型：\"\r\n                          prop=\"type\">\r\n              <el-select v-model=\"deliveryData.type\"\r\n                         class=\"input-style\">\r\n                <el-option v-for=\"item in deliveryType\"\r\n                           :key=\"item.id\"\r\n                           :label=\"item.name\"\r\n                           :value=\"item.id\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"目的地：\"\r\n                          prop=\"endStorageId\"\r\n                          v-if=\"deliveryData.type == 0\">\r\n              <el-select v-model=\"deliveryData.endStorageId\"\r\n                         placeholder=\"请选择目的地\"\r\n                         class=\"input-style\">\r\n                <el-option v-for=\"item in warehouseFilterList\"\r\n                           :key=\"item.id\"\r\n                           :label=\"item.name\"\r\n                           :value=\"item.id\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"顾客姓名：\"\r\n                          prop=\"customer\"\r\n                          v-if=\"deliveryData.type == 1\">\r\n              <el-input v-model=\"deliveryData.customer\"\r\n                        placeholder=\"请输入顾客姓名\"\r\n                        clearable\r\n                        class=\"input-style\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"出售币种：\" v-if=\"deliveryData.type == 1\"\r\n                          prop=\"currency\">\r\n              <el-radio-group v-model=\"deliveryData.saleCurrency\">\r\n                <el-radio label=\"CNY\">CNY</el-radio>\r\n                <el-radio label=\"EUR\">EUR</el-radio>\r\n                <el-radio label=\"USD\">USD</el-radio>\r\n                <el-radio label=\"HKD\">HKD</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"出售金额：\"\r\n                          prop=\"saleMoney\"\r\n                          v-if=\"deliveryData.type == 1\">\r\n              <el-input v-model=\"deliveryData.saleMoney\"\r\n                        placeholder=\"请输入出售金额\"\r\n                        clearable\r\n                        class=\"input-style\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"出库时间：\"\r\n                          prop=\"stockOutTime\">\r\n              <el-date-picker class=\"input-style\"\r\n                              v-model=\"deliveryData.stockOutTime\"\r\n                              type=\"date\"\r\n                              placeholder=\"请选择出库时间\"\r\n                              value-format=\"yyyy-MM-dd\"\r\n                              format=\"yyyy-MM-dd\">\r\n              </el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"领货人：\"\r\n                          prop=\"receiver\">\r\n              <el-input class=\"input-style\"\r\n                        v-model=\"deliveryData.receiver\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"备注：\"\r\n                          prop=\"note\">\r\n              <el-input type=\"textarea\"\r\n                        class=\"input-style\" placeholder=\"请输入备注信息\"\r\n                        v-model=\"deliveryData.note\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div style=\"margin-top: 30px;text-align: right;\">\r\n            <el-button type=\"primary\"\r\n                       @click=\"stockInSubmit('deliveryForm')\"\r\n                       v-preventClick>提 交</el-button>\r\n          </div>\r\n        </div>\r\n        <div class=\"add-right\">\r\n          <div >\r\n            <p class=\"title-font\">加工中原材料库存查询</p>\r\n            <div>\r\n                <el-form label-width=\"110px\">\r\n                    <!-- 用户管理仓库 -->\r\n                  <el-form-item label=\"仓库/加工厂：\"\r\n                                required>\r\n                    <el-select v-model=\"storageIdList\"\r\n                               multiple\r\n                               placeholder=\"可多选\"\r\n                               @change=\"storageIdChangeList\">\r\n                      <el-option v-for=\"item in warehouseList\"\r\n                                 v-show=\"item.isOwn==1||item.id==0\"\r\n                                 :key=\"item.id\"\r\n                                 :label=\"item.name\"\r\n                                 :value=\"item.id\">\r\n                      </el-option>\r\n                    </el-select>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"关键字：\">\r\n                    <el-input style=\"width: 66%;\"\r\n                              v-model=\"proudctkeyword\"\r\n                              @change=\"keywordChange\"\r\n                              placeholder=\"设计稿名称,产品备注，产品编号\"\r\n                              clearable>\r\n                    </el-input>  <el-button type=\"primary\"\r\n                             @click=\"warehouseCheck\">查 询</el-button>\r\n                  </el-form-item>              \r\n                </el-form>\r\n            </div>\r\n          </div>\r\n          <el-table ref=\"singleTable\"\r\n                    :data=\"deviceFilterList\"\r\n                    highlight-current-row\r\n                    @row-click=\"handleDeviceListChange\"\r\n                    style=\"width: 100%\"\r\n                    border>\r\n            <el-table-column prop=\"designName\" label=\"设计图名称\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{scope.row.designName==\"\"?'---':scope.row.designName}}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"productNumber\"\r\n                             label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"designName\" label=\"现存仓库\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{scope.row.storageName}}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n              <template>\r\n                <el-button type=\"text\">查看详情</el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <el-dialog id=\"msg\"\r\n                     title=\"成品信息\"\r\n                     :visible.sync=\"dialogInventoryCheckVisible\"\r\n                     :close-on-press-escape=\"false\"\r\n                     :close-on-click-modal=\"false\"\r\n                     :modal-append-to-body=\"false\"\r\n                     :append-to-body=\"false\">\r\n                  <div id=\"invenSome\">\r\n                    <el-tabs\r\n                      style=\"margin-top: 20px;\"\r\n                      v-model=\"activeStockName\"\r\n                    >\r\n                      <el-tab-pane label=\"成品信息\" name=\"first\">\r\n                   <div style=\"display: flex;justify-content: space-between;\">\r\n                    <p class=\"font-title-style\"\r\n                       style=\"margin-top: 0;\">\r\n                      成品信息\r\n                    </p>\r\n                    <div style=\"padding-top: 6px;\">\r\n                      <img style=\"cursor: pointer;\"\r\n                           title=\"修改成品信息\"\r\n                           src=\"../assets/imgs/home/update.png\"\r\n                           @click=\"updateDevice\" />\r\n                    </div>\r\n                  </div>\r\n                        <div class=\"font-div\">\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight:'normal' , }\">\r\n                              设计名称：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"inventoryCheckDetail.designName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color: '#606266',\r\n                          fontWeight: 'normal' , }\">\r\n                              设计师：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"inventoryCheckDetail.designerName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?'#606266': inventoryCheckDetail.isUpdate==0?'#3d82fe':'#606266',\r\n                          fontWeight: isUpdate == 0 ? 'normal' :  inventoryCheckDetail.isUpdate==0?'bold':'normal', }\">\r\n                              唯一编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"inventoryCheckDetail.productNumber\"\r\n                                      :readonly=\"isUpdate == 0 ? true : inventoryCheckDetail.isUpdate==0?false:true\">\r\n                            </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"inventoryCheckDetail.state==4\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight: 'normal' ,}\">\r\n                              客户姓名\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input readonly\r\n                                        v-model=\"inventoryCheckDetail.customer\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\" inventoryCheckDetail.state==4\"   >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color:'#606266',\r\n                          fontWeight:'normal',}\">\r\n                              出售币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group v-model=\"inventoryCheckDetail.saleCurrency\"\r\n                                              disabled>\r\n                                <el-radio label=\"CNY\">CNY</el-radio>\r\n                                <el-radio label=\"EUR\">EUR</el-radio>\r\n                                <el-radio label=\"USD\">USD</el-radio>\r\n                                <el-radio label=\"HKD\">HKD</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"\r\n                              v-show=\" inventoryCheckDetail.state==4\">\r\n                            <div class=\"one-left\"\r\n                                :style=\"{color: '#606266',fontWeight:  'normal' }\">\r\n                              出售金额\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input readonly\r\n                                        v-model=\"inventoryCheckDetail.saleMoney\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"    >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color:'#606266',\r\n                          fontWeight:'normal',}\">\r\n                              金额币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group v-model=\"inventoryCheckDetail.currency\"\r\n                                              disabled>\r\n                                <el-radio label=\"CNY\">CNY</el-radio>\r\n                                <el-radio label=\"EUR\">EUR</el-radio>\r\n                                <el-radio label=\"USD\">USD</el-radio>\r\n                                <el-radio label=\"HKD\">HKD</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              加工费\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input  :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"inventoryCheckDetail.processCost\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"  >\r\n                            <div class=\"one-left\"      :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              成本价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input :readonly=\"isUpdate == 0 ? true : false\" v-model=\"inventoryCheckDetail.costPrice\" clearable=\"\"></el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"  >\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\" >\r\n                              最低销售价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"inventoryCheckDetail.lowestSellPrice\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              建议销售价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"inventoryCheckDetail.adviseSellPrice\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"   >\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              标签价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"inventoryCheckDetail.tagPrice\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"  >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              建议批发价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"inventoryCheckDetail.adviceWholesalePrice\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"  >\r\n            