   },\n        {\n          id: 4,\n          name: \"已出售\",\n        },\n      ],\n      storageList: [\n        {\n          id: 0,\n          name: \"全部\",\n          type: 0,\n        },\n      ],\n      storageIdList: [0],\n      time: \"\",\n\n      siteSel: 0,\n\n      stockInMsg: \"数据加载中...\",\n      stockInList: [],\n      stockListPage: [],\n      total: 0,\n      page: 1,\n      pageNum: 5,\n      keyword: \"\",\n\n      dialogDeviceDetailVisible: false,\n      deviceDetail: {},\n      isUpdate: 0,\n      imgList: [],\n      activeStockName: \"first\",\n\n      imgMaterialList: [],\n\n      startTime: \"\",\n      endTime: \"\",\n    };\n  },\n\n  created() {\n    this.cliHeight = document.body.clientHeight - 185;\n    this.warehouseKeywordCheck();\n    this.getWarehouseFactoryList();\n  },\n\n  methods: {\n    backToList() {\n      alert(this.stockInquiryParam.sel);\n      if (this.stockInquiryParam.sel == 0) {\n        this.siteSel = 1;\n        this.stockInquiryParam.sel = 0;\n      } else if (this.stockInquiryParam.sel == 1) {\n        this.siteSel = 0;\n        this.stockInquiryParam.sel = 1;\n      }\n    },\n    // 图形展示仓库/加工厂下的材料列表\n    imgInquiryDetails(list) {\n      this.imgMaterialList = [];\n      this.imgMaterialList = list;\n      this.siteSel = 1;\n    },\n    backToImg() {\n      this.siteSel = 0;\n    },\n    // 图表切换\n    listOrChart() {\n      if (this.stockInquiryParam.sel == 0) {\n        this.stockInquiryParam.sel = 1;\n      } else if (this.stockInquiryParam.sel == 1) {\n        this.stockInquiryParam.sel = 0;\n      }\n    },\n    // 模糊搜索\n    warehouseKeywordCheck() {\n      this.stockInMsg = \"数据加载中...\";\n      this.stockInList = [];\n      this.$axios\n        .post(this.$store.state.baseUrl + \"/stockMaterialList\", {\n          state: this.state,\n          storageIdList:\n            this.storageIdList.indexOf(0) == -1 ? this.storageIdList : [],\n          startTime:\n            this.startTime == \"\"\n              ? this.time[0]\n              : this.shellDateTwo(this.startTime),\n          endTime:\n            this.endTime == \"\" ? this.time[1] : this.shellDateTwo(this.endTime),\n          keyword: this.keyword,\n        })\n        .then((res) => {\n          console.log(\"原材料库存列表查询\");\n          console.log(res);\n          this.total = res.data.total;\n          this.stockInList = res.data.warehouseFactoryList;\n          if (this.stockInList.length == 0) {\n            this.stockInMsg = \"暂无数据\";\n          } else {\n            this.getList();\n          }\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    // 查看材料详情\n    checkDeviceDetails(val) {\n      this.deviceDetail = {};\n      this.activeStockName = \"first\";\n      this.isUpdate = 0;\n      this.$axios\n        .post(this.$store.state.baseUrl + \"/stockMaterialInfo\", {\n          id: val.id,\n        })\n        .then((res) => {\n          console.log(\"材料详情\");\n          console.log(res);\n          this.deviceDetail = res.data;\n          this.imgList = [];\n          if (this.deviceDetail.img !== null && this.deviceDetail.img !== \"\") {\n            if (this.deviceDetail.img.indexOf(\"|\") !== -1) {\n              this.imgList = this.deviceDetail.img.split(\"|\");\n            } else {\n              this.imgList.push(this.deviceDetail.img);\n            }\n          } else {\n            this.imgList = [];\n          }\n          this.dialogDeviceDetailVisible = true;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n    // 查看材料详情\n    checkDeviceDetailsTwo(val) {\n      this.deviceDetail = {};\n      this.activeStockName = \"first\";\n      this.isUpdate = 0;\n      this.$axios\n        .post(this.$store.state.baseUrl + \"/stockMaterialInfo\", {\n          id: val.id,\n        })\n        .then((res) => {\n          console.log(\"材料详情\");\n          console.log(res);\n          this.deviceDetail = res.data;\n          this.img