CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Stock-inquiry.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"stock-inquiry-container\" id=\"stock-inquiry\">\n    <!-- 库存查询 -->\n    <div class=\"stock-inquiry-pc\">\n      <div v-if=\"siteSel == 0\">\n        <div class=\"stock-inquiry-top\" id=\"stockWarehouseCheck\">\n          <el-form :inline=\"true\" class=\"demo-form-inline\">\n            <el-form-item label=\"状态：\">\n              <el-select\n                v-model=\"state\"\n                placeholder=\"请选择\"\n                style=\"width: 150px;\"\n              >\n                <el-option\n                  v-for=\"flag in stateList\"\n                  :key=\"flag.id\"\n                  :label=\"flag.name\"\n                  :value=\"flag.id\"\n                ></el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"仓库/加工厂：\">\n              <el-select\n                v-model=\"storageIdList\"\n                multiple\n                placeholder=\"可多选\"\n                style=\"width: 150px;\"\n              >\n                <el-option\n                  v-for=\"item in storageList\"\n                  :key=\"item.id\"\n                  :label=\"item.name\"\n                  :value=\"item.id\"\n                ></el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"入库时间：\">\n              <el-date-picker\n                v-model=\"time\"\n                type=\"daterange\"\n                range-separator=\"至\"\n                start-placeholder=\"开始日期\"\n                end-placeholder=\"结束日期\"\n                format=\"yyyy-MM-dd\"\n                value-format=\"yyyy-MM-dd\"\n              >\n              </el-date-picker>\n            </el-form-item>\n            <el-form-item>\n              <div style=\"display: flex;\">\n                <el-input\n                  style=\"width: 350px;\"\n                  v-model=\"keyword\"\n                  placeholder=\"可输入材料名称、统一编号、备注进行搜索\"\n                  clearable\n                >\n                </el-input>\n                <el-button\n                  style=\"margin-left: 10px;\"\n                  type=\"primary\"\n                  @click=\"warehouseKeywordCheck\"\n                  >查 询</el-button\n                >\n              </div>\n            </el-form-item>\n          </el-form>\n          <div class=\"top-right\">\n            <el-tooltip\n              class=\"item\"\n              style=\"margin-left: 20px;height: 40px;\"\n              effect=\"dark\"\n              content=\"图表切换\"\n              placement=\"bottom-end\"\n            >\n              <img\n                :src=\"stockInquiryParam.sel == 0 ? img1 : img2\"\n                @click=\"listOrChart\"\n                style=\"cursor:pointer;\"\n              />\n            </el-tooltip>\n          </div>\n        </div>\n        <div class=\"stock-inquiry-main\">\n          <div v-if=\"stockInquiryParam.sel == 0\" class=\"svg-style\">\n            <!-- <h3>图形</h3> -->\n            <div style=\"text-align: center;\" v-if=\"stockInList.length == 0\">\n              <p>{{ stockInMsg }}</p>\n            </div>\n            <div v-else class=\"border-every-style\">\n              <div\n                v-for=\"(warehouse, index) in stockInList\"\n                :key=\"index\"\n                style=\"display: flex;margin-top: 20px;\"\n              >\n                <div\n                  class=\"border-style\"\n                  @click=\"imgInquiryDetails(warehouse.materialList)\"\n                >\n                  {{ warehouse.storageName }}\n                </div>\n                <div\n                  class=\"every-style\"\n                  v-if=\"index < stockInList.length - 1\"\n                ></div>\n              </div>\n            </div>\n          </div>\n          <div v-if=\"stockInquiryParam.sel == 1\" style=\"padding: 20px;\">\n            <div style=\"text-align: center;\" v-if=\"stockInList.length == 0\">\n              <p>{{ stockInMsg }}</p>\n            </div>\n            <div v-else>\n              <div v-for=\"(every, index) in stockListPage\" :key=\"index\">\n                <div style=\"display: flex;text-align: left;\">\n                  <p style=\"font-size: 18px;font-weight: bold;\">\n                    仓库/加工厂： {{ every.storageName }}\n                  </p>\n                  <p style=\"margin: 24px 0 0 20px;font-size: 13px;color: #bbb;\">\n                    数量：{{ every.materialList.length }}\n                  </p>\n                </div>\n                <el-table\n                  style=\"width: 100%;\"\n                  ref=\"multipleTable\"\n                  :data=\"every.materialList\"\n                  tooltip-effect=\"dark\"\n                  border\n                  highlight-current-row\n                  @row-click=\"checkDeviceDetails\"\n                >\n                  <el-table-column prop=\"materialName\" label=\"材料名称\">\n                  </el-table-column>\n                  <el-table-column prop=\"unifiedNumber\" label=\"统一编号\">\n                  </el-table-column>\n                  <el-table-column label=\"单价\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{\n                          scope.row.unitPrice + \" \" + scope.row.currency\n                        }}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"总价\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{\n                          formatNumberRgx(scope.row.totalPrice) +\n                            \" \" +\n                            scope.row.currency\n                        }}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"库存总数量\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{\n                          scope.row.chargeUnit !== \"粒\"\n                            ? scope.row.number\n                            : scope.row.number + \" \" + scope.row.chargeUnit\n                        }}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"库存总重量\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{\n                          scope.row.chargeUnit == \"粒\"\n                            ? scope.row.weight\n                            : scope.row.weight + \" \" + scope.row.chargeUnit\n                        }}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                </el-table>\n                <el-dialog\n                  title=\"材料信息\"\n                  :visible.sync=\"dialogDeviceDetailVisible\"\n                  :close-on-press-escape=\"false\"\n                  :close-on-click-modal=\"false\"\n                  :modal-append-to-body=\"false\"\n                  :append-to-body=\"false\"\n                >\n                  <div id=\"invenSome\">\n                    <el-tabs\n                      style=\"margin-top: 20px;\"\n                      v-model=\"activeStockName\"\n                    >\n                      <el-tab-pane label=\"材料信息\" name=\"first\">\n                        <p class=\"font-title-style\" style=\"margin-top: 0;\">\n                          材料信息\n                        </p>\n                        <div class=\"font-div\">\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              材料名称\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.materialName }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              仓库/加工厂\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.storageName }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              统一编号\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.unifiedNumber }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              形状\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.shape }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              计价单位\n                            </div>\n                            <div class=\"one-right\">\n                              <el-radio-group\n                                v-model=\"deviceDetail.chargeUnit\"\n                                :disabled=\"true\"\n                              >\n                                <el-radio label=\"粒\">粒</el-radio>\n                                <el-radio label=\"ct\">ct</el-radio>\n                                <el-radio label=\"g\">g</el-radio>\n                              </el-radio-group>\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              单价\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.unitPrice }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              币种\n                            </div>\n                            <div class=\"one-right\">\n                              <el-radio-group\n                                v-model=\"deviceDetail.currency\"\n                                :disabled=\"true\"\n                              >\n                                <el-radio label=\"CNY\">CNY</el-radio>\n                                <el-radio label=\"EUR\">EUR</el-radio>\n                                <el-radio label=\"USD\">USD</el-radio>\n                                <el-radio label=\"HKD\">HKD</el-radio>\n                              </el-radio-group>\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              总数量\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.number }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              总重量\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.weight }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              总价\n                            </div>\n                            <div class=\"one-right\">\n                              {{\n                                deviceDetail.totalPrice +\n                                  \" \" +\n                                  deviceDetail.currency\n                              }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              零售单价\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.retailPrice }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              批发单价\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.wholesalePrice }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              尺寸/毫米\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.size }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              颜色\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.color }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              配对\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.paired }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              品质\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.quality }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              品质细分\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.subsection }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              盒子编号\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.boxNumber }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              证书编号\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.certificateNumber }}\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              产品编号\n                            </div>\n                            <div class=\"one-right\">\n                              {{ deviceDetail.productNumber }}\n                            </div>\n                          </div>\n                        </div>\n                        <p class=\"font-title-style\">图片信息</p>\n                        <div class=\"upload-imgs\">\n                          <!-- <div class=\"add\">\n                            <form enctype=\"multipart/form-data\" style=\"width: 100px;\">\n                              <input @change=\"inputChange($event)\" type=\"file\" name=\"img\" accept=\"image/*\"\n                                class=\"inputUpload\" :readonly=\"true\" />\n                              <i class=\"el-icon-plus addIcon\"></i>\n                            </form>\n                          </div> -->\n                          <div\n                            style=\"display:flex;position:relative;flex-wrap: wrap;\"\n                            id=\"delImg\"\n                            class=\"delImg\"\n                          >\n                            <div\n                              v-for=\"(imgurl, index) of imgList\"\n                              :key=\"index\"\n                              style=\"margin-left:10px;position:relative;\"\n                            >\n                              <!-- <span v-if=\"imgurl !== '' && imgurl !== null\" class=\"spanStyle\">x</span> -->\n                              <img\n                                v-if=\"imgurl !== '' && imgurl !== null\"\n                                :src=\"img + '/file/' + imgurl\"\n                                width=\"100px\"\n                                height=\"100px\"\n                                style=\"border-radius:5px;object-fit:cover;\"\n                              />\n                            </div>\n                          </div>\n                        </div>\n                        <p class=\"font-title-style\">备注信息</p>\n                        <div class=\"font-div\">\n                          <div class=\"div-one\">\n                            <div class=\"one-left\">\n                              备注\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input\n                                type=\"textarea\"\n                                v-model=\"deviceDetail.note\"\n                                readonly\n                              >\n                              </el-input>\n                            </div>\n                          </div>\n                        </div>\n                      </el-tab-pane>\n                      <el-tab-pane label=\"历史记录\" name=\"second\">\n                        <p class=\"font-title-style\" style=\"margin-top: 0;\">\n                          材料状态：{{\n                            deviceDetail.state == 0\n                              ? \"库存中\"\n                              : deviceDetail.state == 1\n                              ? \"运输中\"\n                              : deviceDetail.state == 2\n                              ? \"加工中\"\n                              : deviceDetail.state == 3\n                              ? \"加工完成\"\n                              : \"已出售\"\n                          }}\n                        </p>\n                        <el-timeline>\n                          <el-timeline-item\n                            color=\"#3d82fe\"\n                            size=\"large\"\n                            v-for=\"(activity,\n                            index) in deviceDetail.stockInOutList\"\n                            :key=\"index\"\n                          >\n                            <div\n                              style=\"display: flex;justify-content: space-between;\"\n                            >\n                              <p style=\"margin: 0;\">\n                                {{ activity.type == 0 ? \"货物转移\" : \"出售\" }}\n                              </p>\n                              <p style=\"margin: 0;\">\n                                <el-popover\n                                  v-if=\"activity.note !== ''\"\n                                  placement=\"bottom-start\"\n                                  title=\"出库备注信息\"\n                                  width=\"200\"\n                                  trigger=\"hover\"\n                                  :content=\"activity.note\"\n                                >\n                                  <el-button slot=\"reference\">备注</el-button>\n                                </el-popover>\n                              </p>\n                            </div>\n                            <p v-html=\"activity.describe\"></p>\n                            <p>出库时间：{{ activity.stockOutTime }}</p>\n                            <p v-if=\"activity.type == 0\">\n                              到达仓库/加工厂时间：{{\n                                activity.stockInTime == \"\" ||\n                                activity.stockInTime == null\n                                  ? \"暂未入库\"\n                                  : activity.stockInTime\n                              }}\n                            </p>\n                          </el-timeline-item>\n                        </el-timeline>\n                      </el-tab-pane>\n                    </el-tabs>\n                  </div>\n                  <div slot=\"footer\">\n                    <el-button @click=\"dialogDeviceDetailVisible = false\"\n                      >取 消</el-button\n                    >\n                    <el-button\n                      type=\"primary\"\n                      @click=\"dialogDeviceDetailVisible = false\"\n                      >确 定</el-button\n                    >\n                  </div>\n                </el-dialog>\n              </div>\n              <div style=\"width: 100%;height: 50px;\">\n                <div style=\"margin:15px 0;position:absolute;right:6%;\">\n                  <el-pagination\n                    @current-change=\"handleCurrentChange\"\n                    :current-page=\"page\"\n                    layout=\"total, prev, pager, next, jumper\"\n                    :total=\"total\"\n                  >\n                  </el-pagination>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div v-else>\n        <div\n          style=\"width: 60px;margin-bottom: 20px;display: flex;justify-content:space-between;cursor: pointer;\"\n          @click=\"backToImg\"\n        >\n          <img src=\"../assets/imgs/home/goback.png\" style=\"height: 21px;\" />\n          <p style=\"margin: 0;\">返回</p>\n        </div>\n        <el-table\n          style=\"width: 70%;\"\n          ref=\"multipleTable\"\n          :data=\"imgMaterialList\"\n          tooltip-effect=\"dark\"\n          border\n          highlight-current-row\n          @row-click=\"checkDeviceDetails\"\n        >\n          <el-table-column prop=\"materialName\" label=\"材料名称\">\n          </el-table-column>\n          <el-table-column prop=\"unifiedNumber\" label=\"统一编号\">\n          </el-table-column>\n          <el-table-column label=\"单价\">\n            <template slot-scope=\"scope\">\n              <div>\n                <span>{{\n                  scope.row.unitPrice + \" \" + scope.row.currency\n                }}</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"总价\">\n            <template slot-scope=\"scope\">\n              <div>\n                <span>{{\n                  formatNumberRgx(scope.row.totalPrice) +\n                    \" \" +\n                    scope.row.currency\n                }}</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"库存总数量\">\n            <template slot-scope=\"scope\">\n              <div>\n                <span>{{\n                  scope.row.chargeUnit !== \"粒\"\n                    ? scope.row.number\n                    : scope.row.number + \" \" + scope.row.chargeUnit\n                }}</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"库存总重量\">\n            <template slot-scope=\"scope\">\n              <div>\n                <span>{{\n                  scope.row.chargeUnit == \"粒\"\n                    ? scope.row.weight\n                    : scope.row.weight + \" \" + scope.row.chargeUnit\n                }}</span>\n              </div>\n            </template>\n          </el-table-column>\n        </el-table>\n        <el-dialog\n          title=\"材料信息\"\n          :visible.sync=\"dialogDeviceDetailVisible\"\n          :close-on-press-escape=\"false\"\n          :close-on-click-modal=\"false\"\n          :modal-append-to-body=\"false\"\n          :append-to-body=\"false\"\n        >\n          <div id=\"invenSome\">\n            <el-tabs style=\"margin-top: 20px;\" v-model=\"activeStockName\">\n              <el-tab-pane label=\"材料信息\" name=\"first\">\n                <p class=\"font-title-style\" style=\"margin-top: 0;\">材料信息</p>\n                <div class=\"font-div\">\n                  <div class=\"div-one\">\n                    <div class=\"one-left\">\n                      材料名称\n                    </div>\n                    <div class=\"one-right\">\n                      {{ deviceDetail.materialName }}\n                    </div>\n                  </div>\n                  <div class=\"div-one\">\n                    <div class=\"one-left\">\n                      仓库/加工厂\n                    </div>\n                    <div class=\"one-right\">\n                      {{ deviceDetail.storageName }}\n                    </div>\n                  </div>\n                  <div class=\"div-one\">\n                    <div class=\"one-left\">\n          