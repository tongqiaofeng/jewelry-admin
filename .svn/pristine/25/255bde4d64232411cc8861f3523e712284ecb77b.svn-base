isplay: flex;\">\n                      <div style=\"margin-top: 3px;\">\n                        <img\n                          src=\"../assets/imgs/update-psw.png\"\n                          style=\"width: 17px;height: 20px;\"\n                        />\n                      </div>\n                      <span class=\"span-psw\">修改密码</span>\n                    </div>\n                  </div>\n                </div>\n                <el-dialog\n                  class=\"uuupdate\"\n                  title=\"修改密码\"\n                  :visible.sync=\"pswDialogVisible\"\n                  :append-to-body=\"true\"\n                  :close-on-press-escape=\"false\"\n                  :close-on-click-modal=\"false\"\n                >\n                  <div style=\"width:100%;display:flex;\">\n                    <p class=\"font\">旧密码：</p>\n                    <input\n                      class=\"update-nick\"\n                      type=\"text\"\n                      v-model=\"oldPsw\"\n                      placeholder=\"请输入旧密码\"\n                    />\n                  </div>\n                  <div style=\"width:100%;margin-top:15px;display:flex;\">\n                    <p class=\"font\">新密码：</p>\n                    <input\n                      class=\"update-nick\"\n                      type=\"text\"\n                      v-model=\"newPsw\"\n                      placeholder=\"请输入新密码\"\n                    />\n                  </div>\n                  <span slot=\"footer\" class=\"dialog-footer\">\n                    <el-button\n                      @click=\"pswDialogVisible = false\"\n                      style=\"margin-left: 35%;\"\n                      >取 消</el-button\n                    >\n                    <el-button type=\"primary\" @click=\"updatePsw\" v-preventClick\n                      >确 定</el-button\n                    >\n                  </span>\n                </el-dialog>\n              </li>\n              <li class=\"upd