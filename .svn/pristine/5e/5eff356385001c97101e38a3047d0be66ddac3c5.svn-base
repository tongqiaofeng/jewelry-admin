A,OAAA;;AACA,OAAA,SAAA,YAAA,GAAA;AACA,YAAA,aAAA,GACA,QAAA,CAAA,eAAA,CAAA,SAAA,IAAA,QAAA,CAAA,IAAA,CAAA,SADA;;AAEA,YAAA,aAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,qBAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,aAAA,GAAA,aAAA,GAAA,CAAA;AACA;AACA,OAPA;AAQA,KA7VA;AA8VA;AACA,IAAA,eA/VA,6BA+VA;AAAA;;AACA,WAAA,MAAA,CACA,IADA,CACA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,eADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OATA;AAUA,KA1WA;AA2WA;AACA,IAAA,uBA5WA,qCA4WA;AAAA;;AACA,WAAA,MAAA,CACA,IADA,CACA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,uBADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,aAAA;;AACA,QAAA,MAAA,CAAA,cAAA;;AACA,QAAA,MAAA,CAAA,mBAAA;AACA,OATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAZA;AAaA,KA1XA;AA2XA;AACA,IAAA,OA5XA,qBA4XA;AACA,UAAA,KAAA,GAAA,IAAA;AACA,MAAA,oBAAA,CAAA,KAAA,CAAA;AACA,MAAA,KAAA,GAAA,qBAAA,CAAA,SAAA,EAAA,GAAA;AACA,YAAA,IAAA,GACA,QAAA,CAAA,IAAA,CAAA,SAAA,IAAA,QAAA,CAAA,eAAA,CAAA,SADA;;AAEA,YAAA,IAAA,GAAA,CAAA,EAAA;AACA,UAAA,QAAA,CAAA,IAAA,CAAA,SAAA,GAAA,QAAA,CAAA,eAAA,CAAA,SAAA,GACA,IAAA,GAAA,GADA;AAEA,UAAA,KAAA,GAAA,qBAAA,CAAA,EAAA,CAAA;AACA,SAJA,MAIA;AACA,UAAA,oBAAA,CAAA,KAAA,CAAA;AACA;AACA,OAVA,CAAA;AAWA;AA1YA;AAxGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"warehouse-management-container\" id=\"warehouse\">\r\n    <!-- <h1>出库管理</h1> -->\r\n    <div style=\"margin-top: 20px;\">\r\n      <div class=\"materials-apply-add\">\r\n        <div class=\"add-left\">\r\n          <p class=\"title-font\">待出库清单</p>\r\n          <el-table\r\n            :data=\"deliveryData.stockInIdList\"\r\n            style=\"width: 100%\"\r\n            border\r\n            id=\"left\"\r\n          >\r\n            <el-table-column prop=\"materialName\" label=\"材料名称\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column label=\"库存总数量\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    scope.row.chargeUnit == \"粒\"\r\n                      ? scope.row.totalNum + \" \" + scope.row.chargeUnit\r\n                      : \"\"\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"库存总重量\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    scope.row.totalWeight == 0\r\n                      ? scope.row.totalWeight\r\n                      : scope.row.totalWeight + \" \" + scope.row.chargeUnit\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"出库数量\" width=\"130\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"num\">\r\n                  <el-input\r\n                    :disabled=\"scope.row.chargeUnit !== '粒' ? true : false\"\r\n                    v-model=\"scope.row.number\"\r\n                    @input=\"changeNum(scope.$index)\"\r\n                    ><i\r\n                      slot=\"suffix\"\r\n                      style=\"color: #606266;margin-right:10%;font-style:normal;\"\r\n                      >{{\r\n                        scope.row.chargeUnit == \"粒\" ? scope.row.chargeUnit : \"\"\r\n                      }}</i\r\n                    >\r\n                  </el-input>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"出库重量\" width=\"130\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"num\">\r\n                  <el-input\r\n                    :disabled=\"scope.row.totalWeight == 0 ? true : false\"\r\n                    v-model=\"scope.row.totalWeight\"\r\n                    :readonly=\"true\"\r\n                    @input=\"changeWeight(scope.$index)\"\r\n                    ><i\r\n                      slot=\"suffix\"\r\n                      style=\"color: #606266;margin-right:10%;font-style:normal;\"\r\n                      >{{\r\n                        scope.row.chargeUnit !== \"粒\"\r\n                          ? scope.row.chargeUnit\r\n                          : \"\"\r\n                      }}</i\r\n                    >\r\n                  </el-input>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"移除材料\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"num\">\r\n                  <el-button\r\n                    type=\"text\"\r\n                    style=\"color: red;\"\r\n                    @click=\"delNum(scope.$index)\"\r\n                    >移除</el-button\r\n                  >\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <el-form\r\n            style=\"margin-top: 20px;\"\r\n            :model=\"deliveryData\"\r\n            :rules=\"deliveryRules\"\r\n            ref=\"deliveryForm\"\r\n            label-width=\"110px\"\r\n          >\r\n            <el-form-item label=\"出库类型：\" prop=\"type\">\r\n              <el-select v-model=\"deliveryData.type\" class=\"input-style\">\r\n                <el-option\r\n                  v-for=\"item in deliveryType\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.name\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item\r\n              label=\"目的地：\"\r\n              prop=\"endStorageId\"\r\n              v-if=\"deliveryData.type == 0\"\r\n            >\r\n              <el-select\r\n                v-model=\"deliveryData.endStorageId\"\r\n                placeholder=\"请选择\"\r\n                class=\"input-style\"\r\n              >\r\n                <el-option\r\n                  v-for=\"item in warehouseFilterList\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.name\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item\r\n              label=\"顾客姓名：\"\r\n              prop=\"customer\"\r\n              v-if=\"deliveryData.type == 1\"\r\n            >\r\n              <el-input\r\n                v-model=\"deliveryData.customer\"\r\n                placeholder=\"请输入\"\r\n                clearable\r\n                class=\"input-style\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item\r\n              label=\"出售金额：\"\r\n              prop=\"saleMoney\"\r\n              v-if=\"deliveryData.type == 1\"\r\n            >\r\n              <el-input\r\n                v-model=\"deliveryData.saleMoney\"\r\n                placeholder=\"请输入\"\r\n                clearable\r\n                class=\"input-style\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item\r\n              label=\"出售金额币种：\"\r\n              prop=\"currency\"\r\n              v-if=\"deliveryData.type == 1\"\r\n            >\r\n              <el-radio-group v-model=\"deliveryData.currency\">\r\n                <el-radio label=\"CNY\">CNY</el-radio>\r\n                <el-radio label=\"EUR\">EUR</el-radio>\r\n                <el-radio label=\"USD\">USD</el-radio>\r\n                <el-radio label=\"HKD\">HKD</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"出库时间：\" prop=\"stockOutTime\">\r\n              <el-date-picker\r\n                class=\"input-style\"\r\n                v-model=\"deliveryData.stockOutTime\"\r\n                type=\"date\"\r\n                placeholder=\"请选择\"\r\n                value-format=\"yyyy-MM-dd\"\r\n                format=\"yyyy-MM-dd\"\r\n              >\r\n              </el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"领货人：\" prop=\"receiver\">\r\n              <el-input\r\n                class=\"input-style\"\r\n                v-model=\"deliveryData.receiver\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"备注：\" prop=\"note\">\r\n              <el-input\r\n                type=\"textarea\"\r\n                class=\"input-style\"\r\n                v-model=\"deliveryData.note\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div style=\"margin-top: 30px;text-align: right;\">\r\n            <el-button type=\"primary\" @click=\"stockInSubmit('deliveryForm')\"\r\n              >提 交</el-button\r\n            >\r\n          </div>\r\n        </div>\r\n        <div class=\"add-right\">\r\n          <div style=\"display: flex;justify-content: space-between;\">\r\n            <p class=\"title-font\">库存查询</p>\r\n            <div>\r\n              <el-button type=\"primary\" @click=\"dialogFilterVisible = true\"\r\n                >筛 选</el-button\r\n              >\r\n              <el-dialog\r\n                title=\"筛选材料\"\r\n                :visible.sync=\"dialogFilterVisible\"\r\n                :close-on-press-escape=\"false\"\r\n                :close-on-click-modal=\"false\"\r\n                :modal-append-to-body=\"false\"\r\n                :append-to-body=\"false\"\r\n              >\r\n                <el-form label-width=\"110px\">\r\n                  <el-form-item label=\"材料名称：\">\r\n                    <el-select\r\n                      v-model=\"materialId\"\r\n                      placeholder=\"请选择\"\r\n                      clearable\r\n                    >\r\n                      <el-option\r\n                        v-for=\"item in materialList\"\r\n                        :key=\"item.id\"\r\n                        :label=\"item.name\"\r\n                        :value=\"item.id\"\r\n                      >\r\n                      </el-option>\r\n                    </el-select>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"仓库/加工厂：\" required>\r\n                    <el-select\r\n                      v-model=\"storageIdList\"\r\n                      placeholder=\"请选择\"\r\n                      @change=\"storageIdChangeList\"\r\n                    >\r\n                      <el-option\r\n                        v-for=\"item in warehouseList\"\r\n                        :key=\"item.id\"\r\n                        :label=\"item.name\"\r\n                        :value=\"item.id\"\r\n                      >\r\n                      </el-option>\r\n                    </el-select>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"入库时间：\">\r\n                    <el-date-picker\r\n                      v-model=\"time\"\r\n                      type=\"daterange\"\r\n                      range-separator=\"至\"\r\n                      start-placeholder=\"开始日期\"\r\n                      end-placeholder=\"结束日期\"\r\n                      format=\"yyyy-MM-dd\"\r\n                      value-format=\"yyyy-MM-dd\"\r\n                      clearable\r\n                    >\r\n                    </el-date-picker>\r\n                  </el-form-item>\r\n                  <el-form-item label=\"关键字：\">\r\n                    <el-input\r\n                      style=\"width: 350px;\"\r\n                      v-model=\"warehouseKeyword\"\r\n                      @change=\"keywordChange\"\r\n                      placeholder=\"可输入材料名称、统一编号、备注进行搜索\"\r\n                      clearable\r\n                    >\r\n                    </el-input>\r\n                  </el-form-item>\r\n                </el-form>\r\n                <div slot=\"footer\">\r\n                  <el-button @click=\"dialogFilterVisible = false\"\r\n                    >取 消</el-button\r\n                  >\r\n                  <el-button type=\"primary\" @click=\"warehouseCheck\"\r\n                    >查 询</el-button\r\n                  >\r\n                </div>\r\n              </el-dialog>\r\n            </div>\r\n          </div>\r\n          <el-table\r\n            ref=\"singleTable\"\r\n            :data=\"deviceFilterList\"\r\n            highlight-current-row\r\n            @row-click=\"handleDeviceListChange\"\r\n            style=\"width: 100%\"\r\n            border\r\n          >\r\n            <el-table-column prop=\"materialName\" label=\"材料名称\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column label=\"单价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    scope.row.unitPrice + \" \" + scope.row.currency\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"总价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    formatNumberRgx(scope.row.totalPrice) +\r\n                      \" \" +\r\n                      scope.row.currency\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"库存总数量\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    scope.row.weight !== 0\r\n                      ? scope.row.number\r\n                      : scope.row.number + \" \" + scope.row.chargeUnit\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"库存总重量\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span>{{\r\n                    scope.row.weight == 0\r\n                      ? scope.row.weight\r\n                      : scope.row.weight + \" \" + scope.row.chargeUnit\r\n                  }}</span>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n              <template>\r\n                <el-button type=\"text\">查看详情</el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <el-dialog\r\n            id=\"msg\"\r\n            title=\"材料信息\"\r\n            :visible.sync=\"dialogInventoryCheckVisible\"\r\n            :close-on-press-escape=\"false\"\r\n            :close-on-click-modal=\"false\"\r\n            :modal-append-to-body=\"false\"\r\n            :append-to-body=\"false\"\r\n          >\r\n            <div id=\"invenSome\">\r\n              <el-tabs style=\"margin-top: -10px;\" v-model=\"activeStockName\">\r\n                <el-tab-pane label=\"材料信息\" name=\"first\">\r\n                  <div style=\"display: flex;justify-content: space-between;\">\r\n                    <p class=\"font-title-style\" style=\"margin-top: 0;\">\r\n                      材料信息\r\n                    </p>\r\n                    <div style=\"padding-top: 6px;\">\r\n                      <img\r\n                        style=\"cursor: pointer;\"\r\n                        title=\"修改材料信息\"\r\n                        src=\"../assets/imgs/home/update.png\"\r\n                   