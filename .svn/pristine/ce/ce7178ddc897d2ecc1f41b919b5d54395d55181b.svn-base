   </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <div style=\"display: flex;\">\r\n            <el-input style=\"width: 330px;\" v-model=\"keyword\" @change=\"keywordChange\" placeholder=\"可输入材料名称、统一编号、备注进行搜索\"\r\n              clearable>\r\n            </el-input>\r\n            <el-button style=\"margin-left: 10px;\" type=\"primary\" @click=\"getStockMaterialList\">查 询</el-button>\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div>\r\n      <el-table style=\"width: 70%;\" ref=\"multipleTable\" :data=\"listPage\" :row-key=\"getRowKeys\" tooltip-effect=\"dark\"\r\n        border @selection-change=\"handleSelectionChange\" @row-click=\"heckDeviceDetailsTwo\">\r\n        <el-table-column type=\"selection\" :reserve-selection=\"true\" width=\"55\" align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"storageName\" label=\"发货仓库/加工厂\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"endStorageName\" label=\"收货仓库/加工厂\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"materialName\" label=\"材料名称\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n        </el-table-column>\r\n        <el-table-column label=\"单价\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.currency!=\"\"?scope.row.unitPrice + ' ' + scope.row.currency:\"--\"}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"发货总数量\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.chargeUnit !== '粒' ? scope.row.number : scope.row.number + ' ' + scope.row.chargeUnit}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"发货总重量\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.chargeUnit == '粒' ? scope.row.weight==0?'---':scope.row.weight :scope.row.chargeUnit == '件' ?\"--\":scope.row.weight + ' ' + scope.row.chargeUnit}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n                <el-dialog v-if=\"materialorproduct==2\"\r\n                  title=\"材料信息\"\r\n                  :visible.sync=\"dialogDeviceDetailVisible\"\r\n                  :close-on-press-escape=\"false\"\r\n                  :close-on-click-modal=\"false\"\r\n                  :modal-append-to-body=\"false\"\r\n                  :append-to-body=\"false\"\r\n                >\r\n                  <div id=\"invenSome\">\r\n                    <el-tabs\r\n                      style=\"margin-top: 20px;\"\r\n                      v-model=\"activeStockName\"\r\n                    >\r\n                      <el-tab-pane label=\"材料信息\" name=\"first\">\r\n                        <p class=\"font-title-style\" style=\"margin-top: 0;\">\r\n                          材料信息\r\n                        </p>\r\n                        <div class=\"font-div\">\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\">\r\n                              材料名称\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.materialName }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\">\r\n                              仓库/加工厂\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.storageName }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" >\r\n                            <div class=\"one-left\">\r\n                              统一编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.productNumber }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              形状\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.shape }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"\r\n                              v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              产品类型\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input v-model=\"deviceDetail.productType\" clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"\r\n                              v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              产地\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input v-model=\"deviceDetail.productArea\" clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              计价单位\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group\r\n                                v-model=\"deviceDetail.chargeUnit\"\r\n                                :disabled=\"true\"\r\n                              >\r\n                                <el-radio label=\"粒\">粒</el-radio>\r\n                                <el-radio label=\"ct\">ct</el-radio>\r\n                                <el-radio label=\"g\">g</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              单价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.unitPrice }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group\r\n                                v-model=\"deviceDetail.currency\"\r\n                                :disabled=\"true\"\r\n                              >\r\n                                <el-radio label=\"CNY\">CNY</el-radio>\r\n                                <el-radio label=\"EUR\">EUR</el-radio>\r\n                                <el-radio label=\"USD\">USD</el-radio>\r\n                                <el-radio label=\"HKD\">HKD</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\">\r\n                              总数量\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{deviceDetail.chargeUnit==\"粒\"?deviceDetail.number+\" \"+deviceDetail.chargeUnit:deviceDetail.number }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              总重量\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{deviceDetail.chargeUnit!=\"粒\"?deviceDetail.weight+\" \"+deviceDetail.chargeUnit:\"--\" }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              总价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{\r\n                                deviceDetail.totalPrice +\r\n                                  \" \" +\r\n                                  deviceDetail.currency\r\n                              }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              零售单价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.retailPrice }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              批发单价\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.wholesalePrice }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              尺寸/毫米\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.size }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              颜色\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.color }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              配对\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.paired }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              品质\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.quality }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              品质细分\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.subsection }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              盒子编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.boxNumber }}\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\r\n                            <div class=\"one-left\">\r\n                              证书编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              {{ deviceDetail.certificateNumber }}\r\n                            </div>\r\n                          </div>\r\n                        <div class=\"div-one\"\r\n                            v-show=\"certificateshow2==false\">\r\n                          <div class=\"one-left\">\r\n                            发票号\r\n                          </div>\r\n                          <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.billNumber\"\r\n                                      clearable>\r\n                            </el-input>\r\n                          </div>\r\n                        </div>\r\n                        </div>\r\n                        <p class=\"font-title-style\">图片信息</p>\r\n                        <div class=\"upload-imgs\">\r\n\r\n                          <div\r\n                            style=\"display:flex;position:relative;\"\r\n                            id=\"delImg\"\r\n                            class=\"delImg\"\r\n                          >\r\n                            <div\r\n                              v-for=\"(imgurl, index) of imgList\"\r\n                              :key=\"index\"\r\n                              style=\"margin-left:10px;position:relative;\"\r\n                            >\r\n                              <img\r\n                                v-if=\"imgurl !== '' && imgurl !== null\"\r\n                                :src=\"imgurl.length<42?img + '/file/' + imgurl:imgurl\"\r\n                                width=\"100px\"\r\n                                height=\"100px\"\r\n                                style=\"border-radius:5px;object-fit:cover;\"\r\n                              />\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <p class=\"font-title-style\">备注信息</p>\r\n                        <div class=\"font-div\">\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\">\r\n                              备注\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input\r\n                                type=\"textarea\"\r\n                                v-model=\"deviceDetail.note\"\r\n                                readonly\r\n                              >\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                      </el-tab-pane>\r\n                      <el-tab-pane label=\"历史记录\" name=\"second\">\r\n                        <p class=\"font-title-style\" style=\"margin-top: 0;\">\r\n                          材料状态：{{\r\n                            deviceDetail.state == 0\r\n                              ? \"库存中\"\r\n                              : deviceDetail.state == 1\r\n                              ? \"运输中\"\r\n                              : deviceDetail.state == 2\r\n                              ? \"加工中\"\r\n                              : deviceDetail.state == 3\r\n                              ? \"加工完成\"\r\n                              : \"已出售\"\r\n                          }}\r\n                        </p>\r\n                        <el-timeline>\r\n                          <el-timeline-item\r\n                            color=\"#3d82fe\"\r\n                            size=\"large\"\r\n                            v-for=\"(activity,\r\n                            index) in deviceDetail.stockInOutList\"\r\n                            :key=\"index\"\r\n                          >\r\n                      <div style=\"display: flex;align-items: center;\">\r\n                        <p style=\"margin:0 20px 14px 0;width: 72px;\">{{ activity.time.slice(0,10) }}</p><p style=\"margin-top:0\"  v-html=\"activity.describe\"></p>\r\n                      </div>\r\n                      <p v-if=\"activity.note !== ''\" style=\"margin: 0;color:#919090;margin-left:92px;word-break: break-all;word-wrap: break-word\">\r\n                          {{activity.note}}\r\n                        </p>\r\n                          </el-timeline-item>\r\n                        </el-timeline>\r\n                      </el-tab-pane>\r\n                    </el-tabs>\r\n                  </div>\r\n                  <div slot=\"footer\">\r\n                    <el-button @click=\"dialogDeviceDetailVisible = false\"\r\n                      >取 消</el-button\r\n                    >\r\n                    <el-button\r\n                      type=\"primary\"\r\n                      @click=\"dialogDeviceDetailVisible = false\"\r\n                      >确 定</el-button\r\n                    >\r\n                  </div>\r\n                </el-dialog>\r\n                <el-dialog v-if=\"materialorproduct==1\"\r\n                  title=\"成品信息\"\r\n                  :visible.sync=\"dialogDeviceDetailVisible\"\r\n                  :close-on-press-escape=\"false\"\r\n                  :close-on-click-modal=\"false\"\r\n                  :modal-append-to-body=\"false\"\r\n                  :append-to-body=\"false\"\r\n                >\r\n                  <div id=\"invenSome\">\r\n                    <el-tabs\r\n                      style=\"margin-top: 20px;\"\r\n                      v-model=\"activeStockName\"\r\n                    >\r\n                      <el-tab-pane label=\"成品信息\" name=\"first\">\r\n                   <div style=\"display: flex;justify-content: space-between;\">\r\n                    <p class=\"font-title-style\"\r\n                       style=\"margin-top: 0;\">\r\n                      成品信息\r\n                    </p>\r\n                  </div>\r\n                        <div class=\"font-div\">\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight:'normal' , }\">\r\n                              设计名称：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.designName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color: '#606266',\r\n                          fontWeight: 'normal' , }\">\r\n                              设计师：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.designerName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: '#606266',\r\n                          fontWeight: 'normal' }\">\r\n                              唯一编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.productNumber\"\r\n                                      readonly>\r\n                            </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"deviceDetail.state==4\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight: 'normal' ,}\">\r\n                              客户姓名\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input readonly\r\n                                        v-model=\"deviceDetail.customer\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\" deviceDetail.state==4\"   >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color:'#606266',\r\n                          fontWeight:'normal',}\">\r\n                              出售币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group v-model=\"deviceDetail.saleCurrency\"\r\n                                              disabled>\r\n                        