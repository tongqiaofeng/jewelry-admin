;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA63EA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"Stock-inquiry.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"stock-inquiry-container\" id=\"stock-inquiry\">\n    <!-- 库存查询 -->\n    <div class=\"stock-inquiry-pc\">\n      <div v-if=\"siteSel == 0\">\n        <div class=\"stock-inquiry-top\" id=\"stockWarehouseCheck\">\n          <el-form :inline=\"true\" class=\"demo-form-inline\">\n            <el-form-item label=\"状态：\">\n              <el-select\n                v-model=\"state\"\n                placeholder=\"请选择\"\n                style=\"width: 150px;\"\n              >\n                <el-option\n                  v-for=\"flag in stateList\"\n                  :key=\"flag.id\"\n                  :label=\"flag.name\"\n                  :value=\"flag.id\"\n                ></el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"仓库/加工厂：\">\n              <el-select\n                v-model=\"storageIdList\"\n                multiple\n                placeholder=\"可多选\"\n                style=\"width: 150px;\"\n                @change=\"storageChange\"\n              >\n                <el-option\n                  v-for=\"item in storageList\" v-show=\"item.isOwn==1\"\n                  :key=\"item.id\"\n                  :label=\"item.name\"\n                  :value=\"item.id\"\n                ></el-option>\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"入库时间：\">\n              <el-date-picker\n                v-model=\"time\"\n                type=\"daterange\"\n                range-separator=\"至\"\n                start-placeholder=\"开始日期\"\n                end-placeholder=\"结束日期\"\n                format=\"yyyy-MM-dd\"\n                value-format=\"yyyy-MM-dd\"\n              >\n              </el-date-picker>\n            </el-form-item>\n            <el-form-item>\n              <div style=\"display: flex;\">\n                <el-input\n                  style=\"width: 350px;\"\n                  v-model=\"keyword\"\n                  placeholder=\"可输入材料名称、统一编号、备注进行搜索\"\n                  clearable\n                >\n                </el-input>\n                <el-button\n                  style=\"margin-left: 10px;\"\n                  type=\"primary\"\n                  @click=\"warehouseKeywordCheck\"\n                  >查 询</el-button\n                >\n              </div>\n            </el-form-item>\n          </el-form>\n          <div class=\"top-right\">\n            <el-tooltip\n              class=\"item\"\n              style=\"margin-left: 20px;height: 40px;\"\n              effect=\"dark\"\n              content=\"图表切换\"\n              placement=\"bottom-end\"\n            >\n              <img\n                :src=\"stockInquiryParam.sel == 0 ? img1 : img2\"\n                @click=\"listOrChart\"\n                style=\"cursor:pointer;\"\n              />\n            </el-tooltip>\n          </div>\n        </div>\n        <div class=\"stock-inquiry-main\">\n          <div v-if=\"stockInquiryParam.sel == 0\" class=\"svg-style\">\n            <!-- <h3>图形</h3> -->\n            <div style=\"text-align: center;\" v-if=\"stockInList.length == 0\">\n              <p>{{ stockInMsg }}</p>\n            </div>\n            <div v-else class=\"border-every-style\">\n              <div\n                v-for=\"(warehouse, index) in stockInList\"\n                :key=\"index\"\n                style=\"display: flex;margin-top: 20px;\"\n              >\n                <div\n                  class=\"border-style\"\n                  @click=\"imgInquiryDetails(warehouse.materialList,index)\"\n                >\n                  {{ warehouse.storageName }}\n                </div>\n                <div\n                  class=\"every-style\"\n                  v-if=\"index < stockInList.length - 1\"\n                ></div>\n              </div>\n            </div>\n          </div>\n          <div v-if=\"stockInquiryParam.sel == 1\" style=\"padding: 20px;\">\n            <div style=\"text-align: center;\" v-if=\"stockInList.length == 0\">\n              <p>{{ stockInMsg }}</p>\n            </div>\n            <div v-else>\n              <div v-for=\"(every, index) in stockListPage\" :key=\"index\">\n                <div style=\"display: flex;text-align: left;\">\n                  <p style=\"font-size: 18px;font-weight: bold;\">\n                    仓库/加工厂： {{ every.storageName }}\n                  </p>\n                  <p style=\"margin: 24px 0 0 20px;font-size: 13px;color: #bbb;\">\n                    数量：{{ every.materialList.length }}\n                  </p>\n                </div>\n                <el-table\n                  style=\"width: 100%;\"\n                  ref=\"multipleTable\"\n                  :data=\"every.materialList\"\n                  tooltip-effect=\"dark\"\n                  border\n                  highlight-current-row\n                  @row-click=\"checkDeviceDetails\"\n                >\n                  <el-table-column prop=\"materialName\" label=\"材料名称\">\n                  </el-table-column>\n                  <el-table-column prop=\"productNumber\" label=\"统一编号\">\n                  </el-table-column>\n                  <el-table-column label=\"单价\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{scope.row.currency!=\"\"?scope.row.unitPrice + \" \" + scope.row.currency:\"--\"}}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"总价\">\n                    <template slot-scope=\"scope\">\n                      <div>\n                        <span>{{scope.row.currency!=\"\"?formatNumberRgx(scope.row.totalPrice) +\" \" +scope.row.currency:\"--\"}}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"库存量\">                    \n                    <template slot-scope=\"scope\">\n                      <div v-if=\"scope.row.chargeUnit=='粒'||scope.row.chargeUnit=='件'\">\n                        <span>{{ scope.row.number + \" \" + scope.row.chargeUnit}}</span>\n                      </div>\n                      <div v-else>\n                        <span>{{\n                          scope.row.currency==\"\"\n                            ? scope.row.weight>=0?scope.row.weight+ scope.row.chargeUnit:'--'\n                            : scope.row.weight + \" \" + scope.row.chargeUnit\n                        }}</span><span>{{scope.row.number>1?\" \"+\"/\"+\" \"+scope.row.number+\"件\":\"\"}}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                  <el-table-column label=\"状态\"> \n                    <template slot-scope=\"scope\">\n                      <div v-for=\"(e,index) in stateList\" :key=\"index\">\n                        <span v-show='scope.row.state==e.id&&e.name!=\"全部\"'>{{e.name}}</span>\n                      </div>\n                    </template>\n                  </el-table-column>\n                </el-table>\n                <el-dialog\n                  title=\"材料信息\"\n                  :visible.sync=\"dialogDeviceDetailVisible\"\n                  :close-on-press-escape=\"false\"\n                  :close-on-click-modal=\"false\"\n                  :modal-append-to-body=\"false\"\n                  :append-to-body=\"false\"\n                >\n                  <div id=\"invenSome\">\n                    <el-tabs\n                      style=\"margin-top: 20px;\"\n                      v-model=\"activeStockName\"\n                    >\n                      <el-tab-pane label=\"材料信息\" name=\"first\">\n\n                   <div style=\"display: flex;justify-content: space-between;\">\n                    <p class=\"font-title-style\"\n                       style=\"margin-top: 0;\">\n                      材料信息\n                    </p>\n                    <div style=\"padding-top: 6px;\">\n                      <img style=\"cursor: pointer;\"\n                           title=\"修改材料信息\"\n                           src=\"../assets/imgs/home/update.png\"\n                           @click=\"updateDevice\" />\n                    </div>\n                  </div>\n                        <div class=\"font-div\">\n                          <div class=\"div-one\">\n                            <div class=\"one-left\" :style=\"{\n                          color: '#606266' ,\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',\n                        }\">\n                              材料名称\n                            </div>\n                            <div class=\"one-right\">\n                            <!-- <el-select v-model=\"deviceDetail.materialId\"\n                                      placeholder=\"请选择\"\n                                      class=\"input-style\"\n                                      disabled>\n                              <el-option v-for=\"item in materialList\"\n                                        :key=\"item.id\"\n                                        :label=\"item.name\"\n                                        :value=\"item.id\"\n                                        @click.native=\"materialUpdateChange(item)\">\n                              </el-option>\n                            </el-select> -->\n                             <el-input v-model=\"deviceDetail.materialName\" readonly></el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\"                           :style=\"{\n                          color:  '#606266' ,\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',\n                        }\">\n                              仓库/加工厂\n                            </div>\n                            <div class=\"one-right\">\n                            <!-- <el-select v-model=\"deviceDetail.storageId\"\n                                      placeholder=\"请选择\"\n                                      class=\"input-style\"\n                                      disabled\n                                      >\n                              <el-option v-for=\"item in storageList\"  v-show=\"item.isOwn==1\"\n                                        :key=\"item.id\"\n                                        :label=\"item.name\"\n                                        :value=\"item.id\">\n                              </el-option>\n                            </el-select> -->\n                            <el-input v-model=\"deviceDetail.storageName\" readonly></el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\" :style=\"{\n                          color:'#606266' ,\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold', }\">\n                              统一编号\n                            </div>\n                            <div class=\"one-right\">\n                            <el-input v-model=\"deviceDetail.productNumber\" readonly>\n                            </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\" :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              形状\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.shape\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\"\n                              v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"\n                                :style=\"{color: isUpdate == 0 ? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\n                              产品类型\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.productType\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\"\n                              v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"\n                                :style=\"{color: isUpdate == 0 ? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\n                              产地\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.productArea\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"                           :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              计价单位\n                            </div>\n                            <div class=\"one-right\">\n                              <el-radio-group v-model=\"deviceDetail.chargeUnit\"\n                                              :disabled=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\">\n                                <el-radio label=\"粒\">粒</el-radio>\n                                <el-radio label=\"ct\">ct</el-radio>\n                                <el-radio label=\"g\">g</el-radio>\n                              </el-radio-group>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\" :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\" >\n                              单价\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.unitPrice\" @input=\"priceTotal\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\"   >\n                            <div class=\"one-left\"  :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              币种\n                            </div>\n                            <div class=\"one-right\">\n                              <el-radio-group v-model=\"deviceDetail.currency\"\n                                              :disabled=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\">\n                                <el-radio label=\"CNY\">CNY</el-radio>\n                                <el-radio label=\"EUR\">EUR</el-radio>\n                                <el-radio label=\"USD\">USD</el-radio>\n                                <el-radio label=\"HKD\">HKD</el-radio>\n                              </el-radio-group>\n                            </div>\n                          </div>\n                          <div class=\"div-one\">\n                            <div class=\"one-left\" :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate==0?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              总数量\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate==0?false:true\"\n                                        v-model=\"deviceDetail.number\"\n                                        clearable><i slot=\"suffix\" v-if=\"deviceDetail.chargeUnit=='粒'\"\n                        style=\"color: #606266;font-style:normal;\">{{deviceDetail.chargeUnit}}</i>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\" >\n                            <div class=\"one-left\"   :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate==0?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold', }\">\n                              总重量\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate==0?false:true\"\n                                      v-model=\"deviceDetail.weight\"\n                                      clearable><i slot=\"suffix\" v-if=\"deviceDetail.chargeUnit!='粒'\"\n                      style=\"color: #606266;font-style:normal;\">{{deviceDetail.chargeUnit}}</i>\n                            </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\" >\n                            <div class=\"one-left\" :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate==0?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              总价\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate==0?false:true\"\n                                        v-model=\"deviceDetail.totalPrice\"\n                                        clearable>\n                                <i slot=\"suffix\"\n                                  style=\"color: #606266;font-style:normal;\">{{ deviceDetail.currency }}</i>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"  :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              零售单价\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.retailPrice\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"  :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              批发单价\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.wholesalePrice\"\n                                        clearable>\n                              </el-input>\n                            </div>\n                          </div>\n                          <div class=\"div-one\" v-show=\"certificateshow2==false\">\n                            <div class=\"one-left\"  :style=\"{\n                          color: isUpdate == 0? '#606266' : deviceDetail.isUpdate!==2?'#3d82fe':'#606266',\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\n                              尺寸/毫米\n                            </div>\n                            <div class=\"one-right\">\n                              <el-input :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate!==2?false:true\"\n                                        v-model=\"deviceDetail.size\"\n                                        clearable>\n                              </el-input>\n                            