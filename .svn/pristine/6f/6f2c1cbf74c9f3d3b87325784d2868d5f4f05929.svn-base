A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"Finished-stock-inquiry.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div class=\"stock-inquiry-container\" id=\"stock-inquiry\">\r\n    <!-- 库存查询 -->\r\n    <div class=\"stock-inquiry-pc\">\r\n      <div v-if=\"siteSel == 0\">\r\n        <!-- 头部查询条件 -->\r\n        <div class=\"stock-inquiry-top\" id=\"stockWarehouseCheck\">\r\n          <el-form :inline=\"true\" class=\"demo-form-inline\">\r\n            <el-form-item label=\"状态：\">\r\n              <el-select\r\n                v-model=\"state\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 150px;\"\r\n              >\r\n                <el-option\r\n                  v-for=\"flag in stateList\"\r\n                  :key=\"flag.id\"\r\n                  :label=\"flag.name\"\r\n                  :value=\"flag.id\"\r\n                ></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"仓库/加工厂：\">\r\n              <el-select\r\n                v-model=\"storageIdList\"\r\n                multiple\r\n                placeholder=\"可多选\"\r\n                style=\"width: 150px;\"\r\n                @change=\"storageChange\"\r\n              >\r\n              <!-- //存在出库记录 -->\r\n                <el-option\r\n                  v-for=\"item in storageList\" v-show=\"item.isOwn!=0\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.name\"\r\n                  :value=\"item.id\"\r\n                ></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"入库时间：\">\r\n              <el-date-picker\r\n                v-model=\"time\"\r\n                type=\"daterange\"\r\n                @change='timechange'\r\n                range-separator=\"至\"\r\n                start-placeholder=\"开始日期\"\r\n                end-placeholder=\"结束日期\"\r\n                format=\"yyyy-MM-dd\"\r\n                value-format=\"yyyy-MM-dd\"\r\n              >\r\n              </el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <div style=\"display: flex;\">\r\n                <el-input\r\n                  style=\"width: 350px;\"\r\n                  v-model=\"keyword\"\r\n                  placeholder=\"可输入统一编号、备注进行搜索\"\r\n                  clearable\r\n                >\r\n                </el-input>\r\n                <el-button\r\n                  style=\"margin-left: 10px;\"\r\n                  type=\"primary\"\r\n                  @click=\"warehouseKeywordCheck\"\r\n                  >查 询</el-button\r\n                >\r\n              </div>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div class=\"top-right\">\r\n            <el-tooltip\r\n              class=\"item\"\r\n              style=\"margin-left: 20px;height: 40px;\"\r\n              effect=\"dark\"\r\n              content=\"图表切换\"\r\n              placement=\"bottom-end\"\r\n            >\r\n              <img\r\n                :src=\"stockInquiryParam.sel == 0 ? img1 : img2\"\r\n                @click=\"listOrChart\"\r\n                style=\"cursor:pointer;\"\r\n              />\r\n            </el-tooltip>\r\n          </div>\r\n        </div>\r\n        <div class=\"stock-inquiry-main\">\r\n          <!-- 图形 -->\r\n          <div v-if=\"stockInquiryParam.sel == 0\" class=\"svg-style\">\r\n            <div style=\"text-align: center;\" v-if=\"stockInList.length==0\">\r\n              <p>{{ stockInMsg }}</p>\r\n            </div>\r\n            <div v-else class=\"border-every-style\">\r\n              <div\r\n                v-for=\"(warehouse, index) in stockInList\"\r\n                :key=\"index\"\r\n                style=\"display: flex;margin-top: 20px;\"\r\n              >\r\n                <div\r\n                  class=\"border-style\"\r\n                  @click=\"imgInquiryDetails(warehouse.productList,index)\"\r\n                >\r\n                  {{ warehouse.storageName }}\r\n                </div>\r\n                <div\r\n                  class=\"every-style\"\r\n                  v-if=\"index < stockInList.length - 1\"\r\n                ></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- 表格 -->\r\n          <div v-if=\"stockInquiryParam.sel == 1\" style=\"padding: 20px;\">\r\n            <div style=\"text-align: center;\" v-if=\"stockInList.length==0\">\r\n              <p>{{ stockInMsg }}</p>\r\n            </div>\r\n            <div v-else>\r\n              <div v-for=\"(every, index) in stockListPage\" :key=\"index\">\r\n                <div style=\"display: flex;text-align: left;\">\r\n                  <p style=\"font-size: 18px;font-weight: bold;\">\r\n                    仓库/加工厂： {{ every.storageName }}\r\n                  </p>\r\n                  <p style=\"margin: 24px 0 0 20px;font-size: 13px;color: #bbb;\">\r\n                    数量：\r\n                    {{ every.productList.length }}\r\n                  </p>\r\n                </div>\r\n                <el-table\r\n                  style=\"width: 100%;\"\r\n                  ref=\"multipleTable\"\r\n                  :data=\"every.productList\"\r\n                  tooltip-effect=\"dark\"\r\n                  border\r\n                  highlight-current-row\r\n                  @row-click=\"checkproductDetails\"\r\n                >\r\n                  <el-table-column prop=\"materialName\" label=\"成品图\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <img :src=\"scope.row.img\" alt=\"\" style=\"width: 100%;\">\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n                  </el-table-column>\r\n                  <el-table-column prop=\"processCost\" label=\"加工费\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.processCost + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>                    \r\n                  </el-table-column>  \r\n                  <el-table-column label=\"成品价\" prop=\"costPrice\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.costPrice + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>  \r\n                  </el-table-column>        \r\n                  <el-table-column label=\"最低销售价\" prop=\"lowestSellPrice\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.lowestSellPrice + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template> \r\n                  </el-table-column>\r\n                  <el-table-column label=\"建议销售价\" prop=\"adviseSellPrice\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.adviseSellPrice + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"标签价\" prop=\"tagPrice\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.tagPrice + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"建议批发价\" prop=\"adviceWholesalePrice\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.adviceWholesalePrice + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column> \r\n                  <el-table-column prop=\"saleCommission\" label=\"销售提成\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span>{{scope.row.saleCommission + \" \" + scope.row.currency}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"出库记录\" prop=\"isOut\">\r\n                    <template slot-scope=\"scope\">\r\n                      <div>\r\n                        <span v-if=\"scope.row.isOut==0\">无出库记录</span>\r\n                        <span v-else>存在出库记录</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>        \r\n                  <el-table-column label=\"状态\"> \r\n                    <template slot-scope=\"scope\">\r\n                      <div v-for=\"(e,index) in stateList\" :key=\"index\">\r\n                        <span v-show='scope.row.state==e.id&&e.name!=\"全部\"'>{{e.name}}</span>\r\n                      </div>\r\n                    </template>\r\n                  </el-table-column>\r\n                  <el-table-column prop=\"note\" label=\"备注\">\r\n                  </el-table-column>\r\n                </el-table>\r\n                <el-dialog\r\n                  title=\"成品信息\"\r\n                  :visible.sync=\"dialogDeviceDetailVisible\"\r\n                  :close-on-press-escape=\"false\"\r\n                  :close-on-click-modal=\"false\"\r\n                  :modal-append-to-body=\"false\"\r\n                  :append-to-body=\"false\"\r\n                >\r\n                  <div id=\"invenSome\">\r\n                    <el-tabs\r\n                      style=\"margin-top: 20px;\"\r\n                      v-model=\"activeStockName\"\r\n                    >\r\n                      <el-tab-pane label=\"成品信息\" name=\"first\">\r\n                   <div style=\"display: flex;justify-content: space-between;\">\r\n                    <p class=\"font-title-style\"\r\n                       style=\"margin-top: 0;\">\r\n                      成品信息\r\n                    </p>\r\n                    <div style=\"padding-top: 6px;\">\r\n                      <img style=\"cursor: pointer;\"\r\n                           title=\"修改成品信息\"\r\n                           src=\"../assets/imgs/home/update.png\"\r\n                           @click=\"updateDevice\" />\r\n                    </div>\r\n                  </div>\r\n                        <div class=\"font-div\">\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight:'normal' , }\">\r\n                              设计名称：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.designName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color: '#606266',\r\n                          fontWeight: 'normal' , }\">\r\n                              设计师：\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.designerName\"\r\n                                      readonly>\r\n                            </el-input>                             \r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?'#606266': deviceDetail.isUpdate==0?'#3d82fe':'#606266',\r\n                          fontWeight: isUpdate == 0 ? 'normal' :  deviceDetail.isUpdate==0?'bold':'normal', }\">\r\n                              唯一编号\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                            <el-input v-model=\"deviceDetail.productNumber\"\r\n                                      :readonly=\"isUpdate == 0 ? true : deviceDetail.isUpdate==0?false:true\">\r\n                            </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\"deviceDetail.state==4\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color:  '#606266',\r\n                          fontWeight: 'normal' ,}\">\r\n                              客户姓名\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input readonly\r\n                                        v-model=\"deviceDetail.customer\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\" v-show=\" deviceDetail.state==4\"   >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color:'#606266',\r\n                          fontWeight:'normal',}\">\r\n                              出售币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group v-model=\"deviceDetail.saleCurrency\"\r\n                                              disabled>\r\n                                <el-radio label=\"CNY\">CNY</el-radio>\r\n                                <el-radio label=\"EUR\">EUR</el-radio>\r\n                                <el-radio label=\"USD\">USD</el-radio>\r\n                                <el-radio label=\"HKD\">HKD</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"\r\n                              v-show=\" deviceDetail.state==4\">\r\n                            <div class=\"one-left\"\r\n                                :style=\"{color: '#606266',fontWeight:  'normal' }\">\r\n                              出售金额\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input readonly\r\n                                        v-model=\"deviceDetail.saleMoney\"\r\n                                        clearable>\r\n                              </el-input>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\"    >\r\n                            <div class=\"one-left\"  :style=\"{\r\n                          color:'#606266',\r\n                          fontWeight:'normal',}\">\r\n                              金额币种\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-radio-group v-model=\"deviceDetail.currency\"\r\n                                              disabled>\r\n                                <el-radio label=\"CNY\">CNY</el-radio>\r\n                                <el-radio label=\"EUR\">EUR</el-radio>\r\n                                <el-radio label=\"USD\">USD</el-radio>\r\n                                <el-radio label=\"HKD\">HKD</el-radio>\r\n                              </el-radio-group>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"div-one\">\r\n                            <div class=\"one-left\" :style=\"{\r\n                          color: isUpdate == 0?     '#606266':'#3d82fe',\r\n                          fontWeight: isUpdate == 0 ? 'normal' : 'bold',}\">\r\n                              加工费\r\n                            </div>\r\n                            <div class=\"one-right\">\r\n                              <el-input  :readonly=\"isUpdate == 0 ? true : false\"\r\n                                        v-model=\"deviceDetail.processCost\"\r\n                                        clearable>\r\n                              </el-input>