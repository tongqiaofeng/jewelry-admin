         <el-col :span=\"12\"  v-show=\"certificateshow==false\">\r\n            <el-form-item label=\"形状：\" prop=\"shape\">\r\n              <el-input v-model=\"stockInFormData.shape\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"计价单位：\" prop=\"chargeUnit\">\r\n              <el-radio-group v-model=\"stockInFormData.chargeUnit\" @change=\"chargeUnitfun()\">\r\n                <el-radio label=\"粒\"></el-radio>\r\n                <el-radio label=\"ct\"></el-radio>\r\n                <el-radio label=\"g\"></el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"单价：\" prop=\"unitPrice\">\r\n              <el-input style=\"width: 100%;\" v-model=\"stockInFormData.unitPrice\" placeholder=\"请输入\" @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"14\">\r\n            <el-form-item label=\"\" prop=\"currency\" label-width=\"0\">\r\n              <el-radio-group style=\"margin-left: 10px;\" v-model=\"stockInFormData.currency\">\r\n                <el-radio label=\"CNY\">CNY</el-radio>\r\n                <el-radio label=\"EUR\">EUR</el-radio>\r\n                <el-radio label=\"USD\">USD</el-radio>\r\n                <el-radio label=\"HKD\">HKD</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"20\" v-if=\"numberorweight==false\">\r\n            <el-form-item label=\"总数量：\" prop=\"number\">\r\n              <el-input v-model=\"stockInFormData.number\" placeholder=\"请输入\" @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"20\"  v-show=\"certificateshow==false&&numberorweight==true\">\r\n            <el-form-item label=\"总重量：\" prop=\"weight\" >\r\n              <el-input v-model=\"stockInFormData.weight\" placeholder=\"请输入\" @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"总价：\" prop=\"totalPrice\">\r\n              <el-input v-model=\"stockInFormData.totalPrice\" placeholder=\"请输入\" readonly><i slot=\"suffix\"\r\n                  style=\"color: #606266;margin-right:5%;font-style:normal;\">{{stockInFormData.currency}}</i></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"零售单价：\" prop=\"retailPrice\">\r\n              <el-input v-model=\"stockInFormData.retailPrice\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"批发单价：\" prop=\"wholesalePrice\">\r\n              <el-input v-model=\"stockInFormData.wholesalePrice\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"尺寸/毫米：\" prop=\"size\">\r\n              <el-input v-model=\"stockInFormData.size\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"颜色：\" prop=\"color\">\r\n              <el-input v-model=\"stockInFormData.color\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"配对：\" prop=\"paired\">\r\n              <el-input v-model=\"stockInFormData.paired\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"  v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"品质：\" prop=\"quality\">\r\n              <el-input v-model=\"stockInFormData.quality\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"品质细分：\" prop=\"subsection\">\r\n              <el-input v-model=\"stockInFormData.subsection\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\" v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"盒子编号：\" prop=\"boxNumber\">\r\n              <el-input v-model=\"stockInFormData.boxNumber\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"证书编号：\" prop=\"certificateNumber\">\r\n              <el-input v-model=\"stockInFormData.certificateNumber\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <!-- <el-row :gutter=\"10\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"产品编号：\" prop=\"productNumber\">\r\n              <el-input v-model=\"stockInFormData.productNumber\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row> -->\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"图片：\" prop=\"imgurls\">\r\n              <div style=\"display:flex;\">\r\n                <div class=\"upload-imgs\">\r\n                  <div class=\"add\">\r\n                    <form enctype=\"multipart/form-data\" style=\"width: 100px;\">\r\n                      <input @change=\"inputChange($event)\" type=\"file\" name=\"img\" accept=\"image/*\"\r\n                        class=\"inputUpload\" />\r\n                      <i class=\"el-icon-plus addIcon\"></i>\r\n                    </form>\r\n                  </div>\r\n                  <div style=\"display:flex;position:relative;flex-wrap: wrap;\" id=\"delImg\" class=\"delImg\">\r\n                    <div v-for=\"(imgurl, index) of stockInFormData.imgurls\" :key=\"index\"\r\n                      style=\"margin-left:10px;position:relative;\">\r\n                      <span v-if=\"imgurl !== '' && imgurl !== null\" class=\"spanStyle\" @click=\"delImage(index)\">x</span>\r\n                      <img v-if=\"imgurl !== '' && imgurl !== null\" :src=\"img + '/file/' + imgurl\" width=\"100px\"\r\n                        height=\"100px\" style=\"border-radius:5px;object-fit:cover;\" />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"备注：\" prop=\"note\">\r\n              <el-input type=\"textarea\" v-model=\"stockInFormData.note\" placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div style=\"margin-top: 30px;text-align: right;\">\r\n        <el-button @click=\"resetForm('stockInForm')\">重 置</el-button>\r\n        <el-button style=\"width: 120px;\" type=\"primary\" @click=\"stockInAdd('stockInForm')\" v-preventClick>提 交\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"user-table\" style=\"display: flex;\">\r\n      <el-table :data=\"productList\" tooltip-effect=\"dark\" border @row-click=\"inventoryLook\">\r\n        <el-table-column prop=\"materialName\" label=\"材料名称\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"productNumber\" label=\"产品名称\">\r\n        </el-table-column>\r\n        <el-table-column label=\"单价\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.unitPrice + ' ' + scope.row.currency}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"总价\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{formatNumberRgx(scope.row.totalPrice) + ' ' + scope.row.currency}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"90px\">\r\n          <template>\r\n            <el-button type=\"text\" style=\"color: red;\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-dialog title=\"材料信息\" :visible.sync=\"dialogInventoryCheckVisible\" :close-on-press-escape=\"false\"\r\n        :close-on-click-modal=\"false\" :modal-append-to-body=\"false\" :append-to-body=\"false\">\r\n        <div id=\"invenSome\">\r\n          <el-tabs style=\"margin-top: 20px;\" v-model=\"activeStockName\">\r\n            <el-tab-pane label=\"材料信息\" name=\"first\">\r\n              <div style=\"display: flex;justify-content: space-between;\">\r\n                <p class=\"font-title-style\" style=\"margin-top: 0;\">材料信息</p>\r\n                <div style=\"padding-top: 6px;\">\r\n                  <img style=\"cursor: pointer;\" title=\"修改材料信息\" src=\"../assets/imgs/home/update.png\"\r\n                    @click=\"updateDevice\" />\r\n                </div>\r\n              </div>\r\n              <div class=\"font-div\">\r\n                <div class=\"div-one\">\r\n                  <div class=\"one-left\"\r\n                    :style=\"{color: isUpdate == 0 ? '#606266' : '#3d82fe',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\r\n                    材料名称\r\n                  </div>\r\n                  <div class=\"one-right\">\r\n                    <el-select v-model=\"inventoryCheckDetail.materialId\" placeholder=\"请选择\" class=\"input-style\"\r\n                      :disabled=\"isUpdate == 0 ? true : false\">\r\n                      <el-option v-for=\"item in materialList\" :key=\"item.id\" :label=\"item.name\" :value=\"item.id\"\r\n                        @click.native=\"materialUpdateChange(item)\">\r\n                      </el-option>\r\n                    </el-select>\r\n                  </div>\r\n                </div>\r\n                <div class=\"div-one\">\r\n                  <div class=\"one-left\"\r\n                    :style=\"{color: isUpdate == 0 ? '#606266' : '#3d82fe',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\r\n                    仓库/加工厂\r\n                  </div>\r\n                  <div class=\"one-right\">\r\n                    <el-select v-model=\"inventoryCheckDetail.storageId\" placeholder=\"请选择\" class=\"input-style\"\r\n                      :disabled=\"isUpdate == 0 ? true : false\">\r\n                      <el-option v-for=\"item in warehouseFactoryList\" :key=\"item.id\" :label=\"item.name\"\r\n                        :value=\"item.id\">\r\n                      </el-option>\r\n                    </el-select>\r\n                  </div>\r\n                </div>\r\n                <div class=\"div-one\">\r\n                  <div class=\"one-left\"\r\n                    :style=\"{color: isUpdate == 0 ? '#606266' : '#3d82fe',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\r\n                    统一编号\r\n                  </div>\r\n                  <div class=\"one-right\">\r\n                    <el-input v-model=\"inventoryCheckDetail.productNumber\"