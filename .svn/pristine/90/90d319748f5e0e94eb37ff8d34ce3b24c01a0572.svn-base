,IAXA,CAWA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,OAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,OAAA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA,CAAA,YAAA;AACA,QAAA,OAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA,QAAA,OAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAlBA,EAmBA,KAnBA,CAmBA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OArBA;AAsBA,KA7zBA;AA8zBA;AACA,IAAA,YA/zBA,0BA+zBA;AAAA;;AACA,WAAA,MAAA,CACA,GADA,CACA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,YADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,OAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;;AACA,QAAA,OAAA,CAAA,YAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA;AAWA,KA30BA;AA40BA,IAAA,aA50BA,2BA40BA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,aAAA,GAAA,OAAA;AACA,KA/0BA;AAg1BA;AACA,IAAA,mBAj1BA,+BAi1BA,GAj1BA,EAi1BA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA,WAAA,IAAA,GAAA,GAAA;AACA,WAAA,YAAA;;AACA,OAAA,SAAA,YAAA,GAAA;AACA,YAAA,aAAA,GACA,QAAA,CAAA,eAAA,CAAA,SAAA,IAAA,QAAA,CAAA,IAAA,CAAA,SADA;;AAEA,YAAA,aAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,qBAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,aAAA,GAAA,aAAA,GAAA,CAAA;AACA;AACA,OAPA;AAQA,KA71BA;AA81BA;AACA,IAAA,YA/1BA,0BA+1BA;AAAA;;AACA,WAAA,MAAA,CACA,GADA,CACA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,UADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,OAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OATA;AAUA;AA12BA;AA7EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"stock-pending-container\">\r\n    <!-- <h1>销售报表</h1> -->\r\n    <div style=\"display:flex;justify-content: space-between;\">\r\n      <el-form :inline=\"true\" label-width=\"95px\">\r\n        <el-form-item label=\"统计币种：\">\r\n          <el-select\r\n            v-model=\"currencySel\"\r\n            placeholder=\"请选择\"\r\n            @change=\"keywordChange\"\r\n            :popper-append-to-body=\"false\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in currencySelList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"产品类别：\">\r\n          <el-select\r\n            v-model=\"productType\"\r\n            placeholder=\"请选择\"\r\n            @change=\"keywordChange\"\r\n            :popper-append-to-body=\"false\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in productTypeList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"销售组：\">\r\n          <el-select\r\n            v-model=\"group\"\r\n            placeholder=\"请选择\"\r\n            @change=\"keywordChange\"\r\n            :popper-append-to-body=\"false\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in groupList\"\r\n              :key=\"item\"\r\n              :label=\"item\"\r\n              :value=\"item\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"销售时间：\">\r\n          <el-date-picker\r\n            v-model=\"time\"\r\n            type=\"monthrange\"\r\n            range-separator=\"至\"\r\n            start-placeholder=\"开始月份\"\r\n            end-placeholder=\"结束月份\"\r\n            format=\"yyyy-MM\"\r\n            value-format=\"yyyy-MM\"\r\n            @change=\"keywordChange\"\r\n            clearable\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button\r\n            style=\"margin-left: 10px;\"\r\n            type=\"primary\"\r\n            @click=\"getSalesList\"\r\n            >查 询</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n      <div>\r\n        <el-button type=\"primary\" @click=\"reportData\">导出数据</el-button>\r\n      </div>\r\n    </div>\r\n    <el-tabs v-model=\"activeTabName\" @tab-click=\"handleClick\">\r\n      <el-tab-pane label=\"可结算\" name=\"first\">\r\n        <div>\r\n          <div\r\n            class=\"total-style\"\r\n            v-if=\"salesList.length > 0 && currencySel == 1\"\r\n          >\r\n            <div class=\"style-every\">总数量：{{ total }}</div>\r\n            <div class=\"style-every\">\r\n              人民币销售额：{{ formatNumberRgx(totalSaleMoney) + \" CNY\" }}\r\n            </div>\r\n            <div class=\"style-every\">\r\n              总利润：{{ formatNumberRgx(totalProfit) + \" CNY\" }}\r\n            </div>\r\n          </div>\r\n          <div\r\n            class=\"total-style\"\r\n            v-if=\"salesList.length > 0 && currencySel == 2\"\r\n          >\r\n            <div class=\"style-every\">总数量：{{ total }}</div>\r\n            <div class=\"style-every\">\r\n              港币销售额：{{ formatNumberRgx(totalSaleMoney) + \" HKD\" }}\r\n            </div>\r\n            <div class=\"style-every\">\r\n              总利润：{{ formatNumberRgx(totalProfit) + \" HKD\" }}\r\n            </div>\r\n          </div>\r\n          <el-table\r\n            style=\"width: 100%;\"\r\n            ref=\"multipleTable\"\r\n            :data=\"salesList\"\r\n            tooltip-effect=\"dark\"\r\n            @row-click=\"handleDeviceListChange\"\r\n            border\r\n          >\r\n            <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column label=\"图片\" width=\"120px\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <img\r\n                    v-if=\"scope.row.img != ''\"\r\n                    :src=\"axiosUrl + '/file/' + imgFillter(scope.row.img)[0]\"\r\n                    style=\"width: 100px;height: 100px;object-fit: cover;\"\r\n                  />\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"productName\" label=\"产品名称\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"type\" label=\"产品类别\"> </el-table-column>\r\n            <el-table-column prop=\"params\" label=\"参数\"></el-table-column>\r\n            <el-table-column prop=\"stockInTime\" label=\"入库时间\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"stockOutTime\" label=\"销售时间\">\r\n            </el-table-column>\r\n            <el-table-column label=\"成本\">\r\n              <template slot-scope=\"scope\">\r\n                <div :style=\"{ color: scope.row.cost == 0 ? 'red' : '#666' }\">\r\n                  {{ formatNumberRgx(scope.row.cost) + \" \"\r\n                  }}{{ currencySel == 1 ? \"CNY\" : \"HKD\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"最低售价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.lowestSellPrice) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"批发价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.adviceWholesalePrice) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"零售价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.adviseSellPrice) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"出库价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{\r\n                    formatNumberRgx(scope.row.saleMoney) +\r\n                      \" \" +\r\n                      scope.row.saleCurrency\r\n                  }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"人民币出库价\" v-if=\"currencySel == 1\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.saleCnyOrHkdMoney) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"港币出库价\" v-if=\"currencySel == 2\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.saleCnyOrHkdMoney) + \" HKD\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"利润\">\r\n              <template slot-scope=\"scope\">\r\n                <div :style=\"{ color: scope.row.profit < 0 ? 'red' : '#666' }\">\r\n                  {{\r\n                    scope.row.profit == 0\r\n                      ? \"--\"\r\n                      : formatNumberRgx(scope.row.profit) +\r\n                        \" \" +\r\n                        (currencySel == 1 ? \"CNY\" : \"HKD\")\r\n                  }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"利润率\">\r\n              <template slot-scope=\"scope\">\r\n                <div\r\n                  :style=\"{ color: scope.row.profitRate < 0 ? 'red' : '#666' }\"\r\n                >\r\n                  {{\r\n                    scope.row.profitRate == 0\r\n                      ? \"--\"\r\n                      : scope.row.profitRate + \"%\"\r\n                  }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"customer\" label=\"买家\"> </el-table-column>\r\n            <el-table-column prop=\"solder\" label=\"销售员\"> </el-table-column>\r\n            <el-table-column prop=\"group\" label=\"组\"> </el-table-column>\r\n            <el-table-column label=\"操作\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <span\r\n                    style=\"color: #3d81fd;cursor: pointer;\"\r\n                    @click=\"delSale(scope.row.id)\"\r\n                    >撤销销售</span\r\n                  >\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <div style=\"width: 100%;height: 50px;\">\r\n            <div style=\"margin:15px 0;position:absolute;right:6%;\">\r\n              <el-pagination\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"page\"\r\n                layout=\"total, prev, pager, next, jumper\"\r\n                :total=\"total\"\r\n              >\r\n              </el-pagination>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"不可结算\" name=\"second\">\r\n        <div>\r\n          <div class=\"total-style\" v-if=\"salesList.length > 0\">\r\n            <div class=\"style-every\">总数量：{{ total }}</div>\r\n          </div>\r\n          <el-table\r\n            style=\"width: 100%;\"\r\n            ref=\"multipleTable\"\r\n            :data=\"salesList\"\r\n            tooltip-effect=\"dark\"\r\n            @row-click=\"handleDeviceListChange\"\r\n            border\r\n          >\r\n            <el-table-column prop=\"productNumber\" label=\"统一编号\">\r\n            </el-table-column>\r\n            <el-table-column label=\"图片\" width=\"120px\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  <img\r\n                    v-if=\"scope.row.img != ''\"\r\n                    :src=\"axiosUrl + '/file/' + imgFillter(scope.row.img)[0]\"\r\n                    style=\"width: 100px;height: 100px;object-fit: cover;\"\r\n                  />\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"productName\" label=\"产品名称\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"type\" label=\"产品类别\"> </el-table-column>\r\n            <el-table-column prop=\"params\" label=\"参数\"></el-table-column>\r\n            <el-table-column prop=\"stockInTime\" label=\"入库时间\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"stockOutTime\" label=\"销售时间\">\r\n            </el-table-column>\r\n            <el-table-column label=\"成本\">\r\n              <template slot-scope=\"scope\">\r\n                <div :style=\"{ color: scope.row.cost == 0 ? 'red' : '#666' }\">\r\n                  {{ formatNumberRgx(scope.row.cost) + \" \"\r\n                  }}{{ currencySel == 1 ? \"CNY\" : \"HKD\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"最低售价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.lowestSellPrice) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"批发价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.adviceWholesalePrice) + \" CNY\" }}\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"零售价\">\r\n              <template slot-scope=\"scope\">\r\n                <div>\r\n                  {{ formatNumberRgx(scope.row.adviseSellPrice