nRules\"\r\n               ref=\"stockInForm\"\r\n               label-width=\"110px\"\r\n               class=\"demo-ruleForm\">\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"材料名称：\"\r\n                          prop=\"materialId\">\r\n              <el-select v-model=\"stockInFormData.materialId\"\r\n                         placeholder=\"请选择\"\r\n                         class=\"input-style\">\r\n                <el-option v-for=\"item in materialList\"\r\n                           :key=\"item.id\"\r\n                           :label=\"item.name\"\r\n                           :value=\"item.id\"\r\n                           @click.native=\"materialChange(item)\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"仓库/加工厂：\"\r\n                          prop=\"storageId\">\r\n              <el-select v-model=\"stockInFormData.storageId\"\r\n                         placeholder=\"请选择\"\r\n                         class=\"input-style\"\r\n                         @change=\"storageChange\">\r\n                <el-option v-for=\"item in warehouseFactoryList\" v-show=\"item.isOwn==1\"\r\n                           :key=\"item.id\"\r\n                           :label=\"item.name\"\r\n                           :value=\"item.id\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"统一编号：\"\r\n                          prop=\"productNumber\">\r\n              <el-input v-model=\"stockInFormData.productNumber\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\"\r\n                  v-show=\"certificateshow==false\">\r\n            <el-form-item label=\"形状：\"\r\n                          prop=\"shape\">\r\n              <el-input v-model=\"stockInFormData.shape\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"产品类型：\"\r\n                          prop=\"productType\">\r\n              <el-input v-model=\"stockInFormData.productType\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\"\r\n                  v-show=\"certificateshow==false\">\r\n            <el-form-item label=\"产地：\"\r\n                          prop=\"productArea\">\r\n              <el-input v-model=\"stockInFormData.productArea \"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"计价单位：\"\r\n                          prop=\"chargeUnit\">\r\n              <el-radio-group v-model=\"stockInFormData.chargeUnit\"\r\n                              @change=\"chargeUnitfun()\">\r\n                <el-radio label=\"粒\"></el-radio>\r\n                <el-radio label=\"ct\"></el-radio>\r\n                <el-radio label=\"g\"></el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"10\">\r\n            <el-form-item label=\"单价：\"\r\n                          prop=\"unitPrice\">\r\n              <el-input style=\"width: 100%;\"\r\n                        v-model=\"stockInFormData.unitPrice\"\r\n                        placeholder=\"请输入\"\r\n                        @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"14\">\r\n            <el-form-item label=\"\"\r\n                          prop=\"currency\"\r\n                          label-width=\"0\">\r\n              <el-radio-group style=\"margin-left: 10px;\"\r\n                              v-model=\"stockInFormData.currency\">\r\n                <el-radio label=\"CNY\">CNY</el-radio>\r\n                <el-radio label=\"EUR\">EUR</el-radio>\r\n                <el-radio label=\"USD\">USD</el-radio>\r\n                <el-radio label=\"HKD\">HKD</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"20\"\r\n                  v-show=\"numberorweight==true\">\r\n            <el-form-item label=\"总数量：\"\r\n                          prop=\"number\">\r\n              <el-input v-model=\"stockInFormData.number\"\r\n                        placeholder=\"请输入\"\r\n                        @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"20\"\r\n                  v-show=\"numberorweight==false\">\r\n            <el-form-item label=\"总数量：\"\r\n                          prop=\"number\">\r\n              <el-input v-model=\"stockInFormData.number\"\r\n                        placeholder=\"请输入\"\r\n                        @input=\"priceTotal\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"总重量：\"\r\n                          prop=\"weight\">\r\n              <el-input v-model=\"stockInFormData.weight\"\r\n                        placeholder=\"请输入\"\r\n                        @input=\"priceTotal\"><i slot=\"suffix\"\r\n                   style=\"color: #606266;margin-right:5%;font-style:normal;\">{{stockInFormData.chargeUnit}}</i></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"总价：\"\r\n                          prop=\"totalPrice\">\r\n              <el-input v-model=\"stockInFormData.totalPrice\"\r\n                        placeholder=\"请输入\"\r\n                        readonly><i slot=\"suffix\"\r\n                   style=\"color: #606266;margin-right:5%;font-style:normal;\">{{stockInFormData.currency}}</i></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"批发单价：\"\r\n                          prop=\"wholesalePrice\">\r\n              <el-input v-model=\"stockInFormData.wholesalePrice\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"零售单价：\"\r\n                          prop=\"retailPrice\">\r\n              <el-input v-model=\"stockInFormData.retailPrice\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"尺寸/毫米：\"\r\n                          prop=\"size\">\r\n              <el-input v-model=\"stockInFormData.size\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"颜色：\"\r\n                          prop=\"color\">\r\n              <el-input v-model=\"stockInFormData.color\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"配对：\"\r\n                          prop=\"paired\">\r\n              <el-input v-model=\"stockInFormData.paired\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"品质：\"\r\n                          prop=\"quality\">\r\n              <el-input v-model=\"stockInFormData.quality\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"品质细分：\"\r\n                          prop=\"subsection\">\r\n              <el-input v-model=\"stockInFormData.subsection\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\"\r\n                v-show=\"certificateshow==false\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"盒子编号：\"\r\n                          prop=\"boxNumber\">\r\n              <el-input v-model=\"stockInFormData.boxNumber\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item label=\"证书编号：\"\r\n                          prop=\"certificateNumber\">\r\n              <el-input v-model=\"stockInFormData.certificateNumber\"\r\n                        placeholder=\"多个证书编号，用逗号隔开\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"18\"\r\n                  v-show=\"certificateshow==false\">\r\n            <el-form-item label=\"发票号：\"\r\n                          prop=\"billNumber \">\r\n              <el-input v-model=\"stockInFormData.billNumber  \"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"图片：\"\r\n                          prop=\"imgurls\">\r\n              <div style=\"display:flex;\">\r\n                <div class=\"upload-imgs\">\r\n                  <div class=\"add\" >\r\n                    <form enctype=\"multipart/form-data\"\r\n                          style=\"width: 100px;\">\r\n                      <input @change=\"inputChange($event)\"\r\n                             type=\"file\"\r\n                             name=\"img\"\r\n                             accept=\"image/*\"\r\n                             class=\"inputUpload\" />\r\n                      <i class=\"el-icon-plus addIcon\"></i>\r\n                    </form>\r\n                  </div>\r\n                  <div style=\"display:flex;position:relative;flex-wrap: wrap;\"\r\n                       id=\"delImg\"\r\n                       class=\"delImg\">\r\n                    <div v-for=\"(imgurl, index) of stockInFormData.imgurls\"\r\n                         :key=\"index\"\r\n                         style=\"margin-left:10px;position:relative;\">\r\n                      <span v-if=\"imgurl !== '' && imgurl !== null\"\r\n                            class=\"spanStyle\"\r\n                            @click=\"delImage(index)\">x</span>\r\n                      <img v-if=\"imgurl !== '' && imgurl !== null\"\r\n                           :src=\"img + '/file/' + imgurl\"\r\n                           width=\"100px\"\r\n                           height=\"100px\"\r\n                           style=\"border-radius:5px;object-fit:cover;\" />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"备注：\"\r\n                          prop=\"note\">\r\n              <el-input type=\"textarea\"\r\n                        v-model=\"stockInFormData.note\"\r\n                        placeholder=\"请输入\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div style=\"margin-top: 30px;text-align: right;\">\r\n        <el-button @click=\"resetForm('stockInForm')\">重 置</el-button>\r\n        <el-button style=\"width: 120px;\"\r\n                   type=\"primary\"\r\n                   @click=\"stockInAdd('stockInForm')\"\r\n                   v-preventClick>提 交\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n    <div class=\"user-table\"\r\n         style=\"display: flex;\">\r\n      <el-table :data=\"productList\"\r\n                tooltip-effect=\"dark\"\r\n                border\r\n                @row-click=\"inventoryLook\">\r\n        <el-table-column prop=\"materialName\"\r\n                         label=\"材料名称\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"productNumber\"\r\n                         label=\"统一编号\">\r\n        </el-table-column>\r\n        <el-table-column label=\"单价\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.currency!=\"\"?scope.row.unitPrice + ' ' + scope.row.currency:\"--\"}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"总价\">\r\n          <template slot-scope=\"scope\">\r\n            <div>\r\n              <span>{{scope.row.currency!=\"\"?formatNumberRgx(scope.row.totalPrice) + ' ' + scope.row.currency:'--'}}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\"\r\n                         width=\"90px\">\r\n          <template>\r\n            <el-button type=\"text\"\r\n                       style=\"color: red;\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-dialog title=\"材料信息\"\r\n                 :visible.sync=\"dialogInventoryCheckVisible\"\r\n                 :close-on-press-escape=\"false\"\r\n                 :close-on-click-modal=\"false\"\r\n                 :modal-append-to-body=\"false\"\r\n                 :append-to-body=\"false\">\r\n        <div id=\"invenSome\">\r\n          <el-tabs style=\"margin-top: 20px;\"\r\n                   v-model=\"activeStockName\">\r\n            <el-tab-pane label=\"材料信息\"\r\n                         name=\"first\">\r\n              <div style=\"display: flex;justify-content: space-between;\">\r\n                <p class=\"font-title-style\"\r\n                   style=\"margin-top: 0;\">材料信息</p>\r\n                <div style=\"padding-top: 6px;\">\r\n                  <img style=\"cursor: pointer;\"\r\n                       title=\"修改材料信息\"\r\n                       src=\"../assets/imgs/home/update.png\"\r\n                       @click=\"updateDevice\" />\r\n                </div>\r\n              </div>\r\n              <div class=\"font-div\">\r\n                <div class=\"div-one\">\r\n                  <div class=\"one-left\"\r\n                       :style=\"{color: isUpdate == 0 ? '#606266' : inventoryCheckDetail.isUpdate==0?'#3d82fe':'#606266',fontWeight: isUpdate == 0 ? 'normal' : 'bold'}\">\r\n                    材料名称\r\n                  </div>\r\n                  <div class=\"one-right\">\r\n                    <el-select v-model=\"inventoryCheckDetail.materialId\"\r\n                               placeholder=\"请选择\"\r\n                               class=\"input-style\"\r\n                           